    function ElementCalculation(selectionId, regularElemId, optionalElemId, sumElemId) {
        var x = document.getElementById(selectionId);
        var opt = x.options[x.selectedIndex];
        if (opt.text == "St" || opt.text == "Sp" || opt.text == "EI") {

            document.getElementById(regularElemId).value = "0";
            document.getElementById(optionalElemId).value = "8";
            document.getElementById(regularElemId).disabled = true;
            document.getElementById(optionalElemId).disabled = true;

        }
        else {
            document.getElementById(regularElemId).disabled = false;
            document.getElementById(optionalElemId).disabled = false;
            if (opt.text == "" || opt.text == null) {
                document.getElementById(optionalElemId).value = "0";
            }
        }
        var sat1_reg_Val = document.getElementById(regularElemId).value;
        var sat1_opt_Val = document.getElementById(optionalElemId).value;
        if (sat1_reg_Val != "" && sat1_opt_Val != "") {

            var val1 = parseFloat(sat1_reg_Val);
            var val2 = parseFloat(sat1_opt_Val);
            if (opt.text == "OT" || opt.text == "St" || opt.text == "Sp" || opt.text == "EI") {

                var total = val1 + val2;
                document.getElementById(sumElemId).value = total.toString();
            }
            else {
                if (val1 + val2 > 8) {
                    var val = 8 - val2;
                    if (val < 0) {
                        document.getElementById(regularElemId).value = (0).toString();
                    }
                    else {
                        document.getElementById(regularElemId).value = (8 - val2).toString();
                    }
                }
                var val11 = parseFloat(document.getElementById(regularElemId).value);
                var val21 = parseFloat(document.getElementById(optionalElemId).value);
                document.getElementById(sumElemId).value = (val11 + val21).toString();
            }

        }
        else {
            document.getElementById(regularElemId).disabled = false;
            document.getElementById(optionalElemId).disabled = false;
            document.getElementById(optionalElemId).value = "";
            if (document.getElementById(regularElemId).value != "") {
                var val11 = parseFloat(document.getElementById(regularElemId).value);
                document.getElementById(sumElemId).value = (val11).toString();
            }
        }
        summary();
    }

