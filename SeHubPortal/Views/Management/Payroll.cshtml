                        {
                            <a class="nav-item nav-link active" id="nav-contact-tab" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false" style="padding-left:125px; padding-right: 125px; display:none" onclick="location.href='@Url.Action("PreselectValidation", "Management", new { payrollID = Model.SelectedPayrollId.Split(';')[0] })';"> Validation</a>
