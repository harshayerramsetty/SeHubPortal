    <script src="https://cdnjs.cloudflare.com/ajax/libs/d3-legend/1.7.0/d3-legend.min.js"></script>

    <script>
        $(document).ready(function () {
            function getUrlVars() {
                var vars = [], hash;
                var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                for (var i = 0; i < hashes.length; i++) {
                    hash = hashes[i].split('=');
                    vars.push(hash[0]);
                    vars[hash[0]] = hash[1];
                }
                return vars;
            }

            var acknowledgment = getUrlVars()["ac"];
            if (acknowledgment == "Corporate") {

                $('#nav-contact').removeClass("show");

                $('#branchMainBody').css('display', 'none');
                $('#CorporateMainBody').css('display', 'block');
                $('#BranchDashboard').css('display', 'none');
                $('#CorporateDashboard').css('display', 'block');
                $("#ToggleFromBranchToCorporate").prop("checked", true);
                $('.locationSectiondisabled').css('display', 'block');

                $('#CorporateText').css('font-weight', '700');
                $('#BranchText').css('font-weight', '100');

                $('#nav-profile-tab').css({ "font-weight": "bold", "color": "black" });
                $('#nav-home-tab').css({ "font-weight": "normal", "color": "#24a0ed" });

                $('#nav-contact-tab').css('display', 'block');
                $('#nav-contact-tab').removeClass("active");


                $('#nav-contact-tab').css("font-weight", "normal");
                $('#nav-profile-tab').css("font-weight", "bold");
                $('#nav-home-tab').css("font-weight", "normal");


                $('#nav-contact-tab').css("color", "#24a0ed");
                $('#nav-profile-tab').css("color", "black");
                $('#nav-home-tab').css("color", "#24a0ed");

                $('#entirePage').css('display', 'block');
            }
            else if (acknowledgment == "Dashboard") {

                $('#nav-contact').removeClass("show");

                $('#nav-profile').removeClass("show");
                $('#nav-home').tab('show');
                $('.commonClass').removeClass('active');
                $('.branchDashboardTotals').addClass('active');

                $('#CorporateText').css('font-weight', '100');
                $('#BranchText').css('font-weight', '700');

                $('#nav-profile-tab').css({ "font-weight": "normal", "color": "#24a0ed" });
                $('#nav-home-tab').css({ "font-weight": "bold", "color": "black" });

                $('#nav-contact-tab').css('display', 'none');
                $('#nav-contact-tab').removeClass("active");

                $('#nav-contact-tab').css("font-weight", "normal");
                $('#nav-profile-tab').css("font-weight", "normal");
                $('#nav-home-tab').css("font-weight", "bold");

                $('#nav-contact-tab').css("color", "#24a0ed");
                $('#nav-profile-tab').css("color", "#24a0ed");
                $('#nav-home-tab').css("color", "black");

                $('#entirePage').css('display', 'block');

            }
            else if (acknowledgment == "DashboardCorporate") {

                $('#nav-contact').removeClass("show");

                $('#branchMainBody').css('display', 'none');
                $('#CorporateMainBody').css('display', 'block');
                $('#BranchDashboard').css('display', 'none');
                $('#CorporateDashboard').css('display', 'block');
                $("#ToggleFromBranchToCorporate").prop("checked", true);
                $('#nav-home').tab('show');
                $('.commonClass').removeClass('active');
                $('.branchDashboardTotals').addClass('active');
                $('.locationSectiondisabled').css('display', 'block');

                $('#CorporateText').css('font-weight', '700');
                $('#BranchText').css('font-weight', '100');

                $('#nav-profile-tab').css({ "font-weight": "normal", "color": "#24a0ed" });
                $('#nav-home-tab').css({ "font-weight": "bold", "color": "black" });

                $('#nav-contact-tab').css('display', 'block');

                $('#nav-contact-tab').css("font-weight", "normal");
                $('#nav-profile-tab').css("font-weight", "normal");
                $('#nav-home-tab').css("font-weight", "bold");


                $('#nav-contact-tab').css("color", "#24a0ed");
                $('#nav-profile-tab').css("color", "#24a0ed");
                $('#nav-home-tab').css("color", "black");

                $('#nav-contact-tab').removeClass("active");
                $('#entirePage').css('display', 'block');

            }
            else if (acknowledgment == "Validate") {
                $('#branchMainBody').css('display', 'none');
                $('#CorporateMainBody').css('display', 'block');
                $('#BranchDashboard').css('display', 'none');
                $('#CorporateDashboard').css('display', 'block');
                $("#ToggleFromBranchToCorporate").prop("checked", true);
                $('.locationSectiondisabled').css('display', 'block');

                $('#CorporateText').css('font-weight', '700');
                $('#BranchText').css('font-weight', '100');

                $('#nav-contact-tab').css({ "font-weight": "bold", "color": "black" });
                $('#nav-profile-tab').css({ "font-weight": "normal", "color": "#24a0ed" });
                $('#nav-home-tab').css({ "font-weight": "normal", "color": "#24a0ed" });

                $('#nav-contact-tab').css('display', 'block');
                $('#nav-contact-tab').removeClass("active");
                $('.nav-tabs a:last').tab('show')

                $('#nav-contact-tab').css("font-weight", "bold");
                $('#nav-profile-tab').css("font-weight", "normal");
                $('#nav-home-tab').css("font-weight", "normal");


                $('#nav-contact-tab').css("color", "black");
                $('#nav-profile-tab').css("color", "#24a0ed");
                $('#nav-home-tab').css("color", "#24a0ed");
                $('#entirePage').css('display', 'block');

            }
            else {
                $('#nav-profile-tab').css({ "font-weight": "bold", "color": "black" });
                $('#nav-home-tab').css({ "font-weight": "normal", "color": "#24a0ed" });
                $('#entirePage').css('display', 'block');
            }
        });

    </script>

