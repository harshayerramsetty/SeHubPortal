                                    <label style="margin: 0px 3px 0px 25px ">Payroll ID :</label>
                                    @using (Html.BeginForm("ChangePayrollID", "Management", FormMethod.Post))
                                    {
                                        @Html.DropDownListFor(m => m.SelectedPayrollId, Model.PayrollIdList, new { @id = "payroll_id_dropdown", @onChange = "this.form.submit()", @style = "text-align:left; margin-top: 0px; width:200px;height:23px;font-size:15px;margin-left:6px;" })
