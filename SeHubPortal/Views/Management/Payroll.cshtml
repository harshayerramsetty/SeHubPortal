                                    }

                                    <label style=" margin: 0px 3px 0px 20px ">Location:</label>
                                    @using (Html.BeginForm("PayrrollChangeLocation", "Management", FormMethod.Post))
                                    {
                                        <div>
                                            @Html.DropDownListFor(m => m.MatchedLocID, Model.MatchedLocs, new { @class = "locationSection", @onChange = "this.form.submit()", @type = "text", @style = "text-align-last: center;width:100px;height:23px;font-size:15px;margin-left:6px;" })
                                            <noscript> <input style="display:none" class="btn btn-success" type="submit" value="Search" /></noscript>
                                        </div>
                                    }
                                </div>
                            </div>

                        </div>


