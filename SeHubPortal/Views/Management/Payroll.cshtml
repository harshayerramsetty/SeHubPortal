<body style="overflow-y: hidden">

    <div class="row mainBody">
        <div class="col-md-12">
            <div id="entirePage" style="display:none">
                @{
                    var count3 = 0;

                    foreach (var loc in Model.corpDashboard)
                    {
                        if (loc.corpSaveStatus == 1 && loc.lockStatusbranch == 1)
                        {
                            count3 = count3 + 1;
                        }
                    }
                }
                <div class="row" style="height:50px; margin:5px 0px 15px 0px; background-color:white">
                    <div class="col-lg-4" style="padding-left:0px;">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb" style="background-color:white;height:50px;margin-top:0px;">
                                <li class="breadcrumb-item"><a href="@Url.Action("Index", "Management")">Management</a></li>
                                <li class="breadcrumb-item " aria-current="page"><a href="@Url.Action("Payroll", "Management", new { locId = Model.MatchedLocID, employeeId = "", payrollID = "" })">Payroll</a></li>
                            </ol>
                        </nav>
                    </div>
                    <div class="col-lg-1"></div>
                    <div class="col-lg-2">
                        @if (Model.SehubAccess.payroll >= 2)
                        {
                            <div class="row" style="padding-top:11px;padding-left:50px;">
                                <label style="margin-right:7px; margin-left:0px">
                                    <h5 style="font-weight:700" id="BranchText">Branch</h5>
                                </label>
                                <div class="custom-control custom-switch">
                                    <input type="checkbox" style="width:60px; height:25px" class="custom-control-input" id="ToggleFromBranchToCorporate">
                                    <label class="custom-control-label" for="ToggleFromBranchToCorporate">
                                        <h5 style="font-weight:100" id="CorporateText">Corporate</h5>
                                    </label>
                                </div>
                            </div>
                        }
                    </div>
                    <div class="col-lg-1"></div>
                    <div class="col-lg-4">
                        <div class="row" style="padding-top:12px;">
                            <div class="col-lg-2"></div>
                            <div class="col-lg-10 text-right">
