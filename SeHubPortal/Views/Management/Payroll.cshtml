    function compareEnterWithAttendance(elementVal,totalVal) {
        console.log("Inside compare with attendance");
        var val1 = document.getElementById(elementVal).value;
        const barChartVals = totalVal;  @*@Html.Raw(Json.Encode(ViewData["TOT3"]));*@
        console.log("val1:" + val1);
        console.log("barChartVals:" + barChartVals);
        if (!barChartVals) {
            console.log("yes");
            document.getElementById(elementVal).style.color = "red";
        }
        else {
            var res = barChartVals.split(":");

            var hours = parseFloat(res[0]);
            var min = parseFloat(res[1]);
            var minutes = ((min * 4) / 60);

            var result = 0;
            if (minutes >= 2) {
                result = hours + 0.5;
            }
            else {
                result = hours;
            }

            var enteredVal = 0;
            if (val1 != null && val1 != "" && val1 != " ") {
                enteredVal = parseFloat(val1);
            }

            if (enteredVal > result) {
                document.getElementById(elementVal).style.color = "red";
            }
            else {
                document.getElementById(elementVal).style.color = "black";
            }

        }
        
    }

