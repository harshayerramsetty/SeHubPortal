@model SeHubPortal.Models.tbl_tire_adjustment
@{
    ViewBag.Title = "Calculator";
    Layout = "~/Views/Shared/_ToolsLayout.cshtml";
}
<!DOCTYPE html>
<html>
<head runat="server">
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap4.min.js"></script>







    @if (Convert.ToInt32(ViewData["payroll"]) > 0)
    {
        <style>
            #PayrollClicked {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["fleetTVT"]) > 0)
    {
        <style>
            #FleetTVTClicked {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["treadTracker"]) > 0)
    {
        <style>
            #treadTrackereClicked {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["library"]) > 0)
    {
        <style>
            #libraryClicked {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["main"]) > 0)
    {
        <style>
            #mainClicked {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["settings"]) > 0)
    {
        <style>
            #SettingsClicked {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["management"]) > 0)
    {
        <style>
            #ManagementClicked {
                display: block !important;
            }
        </style>
    }





    @if (Convert.ToInt32(ViewData["fuel_log"]) > 0)
    {
        <style>
            #FuelLogLink {
                display: block !important;
            }
        </style>
    }

    @if (Convert.ToInt32(ViewData["crm"]) > 0)
    {
        <style>
            #CRMLink {
                display: block !important;
            }
        </style>
    }



    <style>

        #DashboardLink {
            display: block !important;
        }

        #toolsClicked {
            display: block !important;
        }

        .noselect {
            -webkit-touch-callout: none; /* iOS Safari */
            -webkit-user-select: none; /* Safari */
            -khtml-user-select: none; /* Konqueror HTML */
            -moz-user-select: none; /* Firefox */
            -ms-user-select: none; /* Internet Explorer/Edge */
            user-select: none; /* Non-prefixed version, currently
                                          supported by Chrome and Opera */
        }

        .tooltip.top .tooltip-inner {
            background-color: red;
        }

        .tooltip.top .tooltip-arrow {
            border-top-color: red;
        }

        #toolsClicked {
            background-color: #f0f2f5;
        }

        #toolsBtn {
            font-weight: bold;
        }

        input[type="email"]::-ms-input-placeholder {
            text-align: center;
        }

        @@media screen {
            #printSection {
                display: none;
            }

            #dontshow {
                display: none;
            }
        }

        @@media print {
            #btnPrint {
                display: none;
            }

            body * {
                visibility: hidden;
            }

            #printSection, #printSection * {
                visibility: visible;
            }

            #printSection {
                position: absolute;
                left: 0;
                top: 0;
                -ms-transform: rotate(270deg) scaleX(1.40) scaleY(1.7);
                -webkit-transform: rotate(270deg) scaleX(1.40) scaleY(1.7);
                transform: rotate(270deg) scaleX(1.40) scaleY(1.7);
                top: 6.3in;
                left: 0.07in;
            }
        }

        #DashboardLink {
            color: black;
            font-weight: 700;
            background-color: #f0f2f5;
        }



        body {
            background-color: #f0f2f5;
            padding-right: 0 !important;
        }

        #myModal {
            padding-right: 32%;
            overflow: hidden;
            margin-right: 0px;
        }

        #myModal1 {
            padding-right: 10%;
            overflow: hidden;
            margin-right: 0px;
        }

        .heading {
            margin: 10px 0px 0px 0px;
        }

            .heading h2 {
                margin-top: 8px;
            }

        .card-header {
            background-color: #cde8ca;
        }

            .card-header h6 {
                color: black;
            }


        .LoadTAformpopup {
            padding-top: 18%;
        }

        .editOrOpen {
            padding-top: 13%;
        }

        .SavedSuccessfully{
            padding-top: 18%;
        }

    </style>

</head>
<body>
    <div class="row heading">
        <div class="col">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb" style="background-color: white;
                  height: 100%;
                  margin-top: 0px;">
                    <li class="breadcrumb-item"><a href="@Url.Action("Calculator", "Tools")">Tools</a></li>
                    <li class="breadcrumb-item " aria-current="page">Dash board</li>
                </ol>
            </nav>
        </div>
        <button type="button" id="clickThis" class="btn" style=" margin-bottom: 14px;">
            <i class="fa fa-bars" style="font-size: 25px "></i>
            <span class="sr-only">Toggle Menu</span>
        </button>
    </div>
    <!-- Modal -->
    <div class="modal fade show in noselect" data-backdrop="static" data-keyboard="false" id="myModal" tabindex="-1" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            @using (Html.BeginForm("TireAdjustment", "Tools", FormMethod.Post))
            {
                <div class="modal-content" style="width:1415px;">
                    <div id="printThis">
                        <div class="row text-center" id="dontshow">
                            <div class="col-md-4"></div>
                            <div class="col-md-4 text-center"><img src="~/Content/LoginLogo.png" / style="margin-left: auto;margin-right: auto; width:400px;"></div>
                            <div class="col-md-4 text-right" style="padding-right:33px; padding-top:40px">
                                <p style="margin: 0px; padding: 0px; line-height : 10px">
                                    <font size="2">
                                        @ViewBag.addressSTREET1, @if (ViewBag.addressSTREET2 != null)
                                        {
                                            @ViewBag.addressSTREET2
                                        }
                                    </font>
                                </p>
                                <p style="margin: 5px 0px 5px 10px; padding: 0px; line-height : 10px"><font size="2">@ViewBag.addressCITY, @ViewBag.addressPROVINCE, @ViewBag.addressPOSTAL</font></p>
                                <p style="margin: 5px 0px 5px 10px; padding: 0px; line-height : 10px"><font size="2">Phone : @ViewBag.locPHONE</font></p>
                                <p style="margin: 5px 0px 5px 10px; padding: 0px; line-height : 10px"><font size="2">Fax : @ViewBag.locFAX</font></p>
                                <p style="margin: 5px 0px 5px 10px; padding: 0px; line-height : 10px"><font size="2">www.citytire.com</font></p>
                            </div>
                        </div>
                        <div class='modal-header' style="padding:0px; margin: 0px">
                            <div class="col-md-8">
                                <div class="row">
                                    <div class="col-lg-3">
                                        <h4 class="text-left" style=" margin-top: 5px ">Tire Adjustment</h4>
                                    </div>
                                    <div class="col-lg-1" style="padding-top:5px">
                                        <div class="row">


                                        </div>

                                    </div>

                                </div>


                            </div>
                            <div class="col-md-4 text-right">
                                <button id="btnPrint" type="button" class="btn btn-primary" style="float:right; display:none; margin-top:5px; width:110px; padding:2px 20px 2px 20px" data-toggle="tooltip" data-placement="top" title="Replacement Invoice is required">
                                    Print
                                </button>
                            </div>
                        </div>
                        <div class="modal-body" style="padding: 15px; height:740px">
                            <input type="text" style="display:none" name="name" id="empName" value="@Model.cta_employee_name" />
                            <input type="text" style="display:none" name="name" id="TAhideID" value="@Model.Tire_Adjustment_id" />
                            <div class="row">
                                <div class="col-md-12">
                                    <table class="table table-bordered" style="border-radius: 5px">
                                        <thead class="thead-dark">
                                            <tr>
                                                <th scope="col" style="font-size:10px; width:250px; padding:0px; margin:0px;" class="text-center">Tire Adjustment Number</th>
                                                <th scope="col" style="font-size:10px; width:160px; padding:0px; margin:0px;" class="text-center">Date</th>
                                                <th scope="col" style="font-size:10px; width:300px; padding:0px; margin:0px;" class="text-center">CTA Employee</th>
                                                <th scope="col" style="font-size:10px; width:250px; padding:0px; margin:0px;" class="text-center">Non-Sig-Number</th>
                                                <th scope="col" style="font-size:10px; width:110px; padding:0px; margin:0px;" class="text-center">Location</th>
                                                <th scope="col" style="font-size:10px; width:250px; padding:0px; margin:0px;" class="text-center">Replacement Invoice / Work Order</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td class="text-center" style="padding:0px">@Html.EditorFor(m => m.Tire_Adjustment_id, new { htmlAttributes = new { @autocomplete = "off", @id = "TAnumber", @readonly = "true", @class = "text-center clear", @required = "required", @style = "border:0px solid transparent; height:28px; width:255px" } })</td>
                                                <td class="text-center" id="TAdate" style="padding:0px">@System.DateTime.Today.Date.ToString("dd/MM/yyyy")</td>
                                                <td class="text-center" id="TAemployee" style="padding:0px">@Model.cta_employee_name</td>
                                                <td class="text-center" id="TAnonSig" style="padding:0px">@Model.non_sig_number</td>
                                                <td class="text-center" id="TAlocation" style="padding:0px">@Model.location_id</td>
                                                <td style="padding:1px">@Html.EditorFor(m => m.replacement_invoice, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @class = "text-center clear", @id = "replacement_invoice", @style = "width:300px; height:28px; border:0px solid transparent;" } })</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="row" style="padding-bottom:10px;">
                                <div class="col-md-4">
                                    <div class="card shadow-sm">
                                        <div class="card-header text-center" style="font-weight:bold; padding:0px">
                                            Customer Information
                                        </div>
                                        <div class="card-body" style="padding: 10px 30px 10px 20px">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Name</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @*Html.EditorFor(m => m.name, new { htmlAttributes = new { @autocomplete = "off", @id = "TAname", @class = "text-center", @required = "required", @style = "height:28px; width:255px" } })*@
                                                    @Html.EditorFor(m => m.name, new { htmlAttributes = new { @id = "TAname", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Phone</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.phone, new { htmlAttributes = new { @autocomplete = "off", @id = "TAphone", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Street</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.street, new { htmlAttributes = new { @autocomplete = "off", @id = "TAstreet", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>City</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.city, new { htmlAttributes = new { @autocomplete = "off", @id = "TAcity", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Province</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.province, new { htmlAttributes = new { @autocomplete = "off", @id = "TAprovince", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Postal Code</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.postal_code, new { htmlAttributes = new { @autocomplete = "off", @id = "TApostalCode", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="card shadow-sm">
                                        <div class="card-header text-center" style="font-weight:bold; padding:0px">
                                            Tire Information
                                        </div>
                                        <div class="card-body" style="padding:10px 30px 10px 20px">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Brand</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @{
                                                        List
                                                        <SelectListItem>
                                                        listItems = new List
                                                        <SelectListItem>
                                                        ();
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "Select Brand",
                                                        });
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "BFGoodrich",
                                                            Value = "BFGoodrich"
                                                        });
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "Dunlop",
                                                            Value = "Dunlop"
                                                        });
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "Goodyear",
                                                            Value = "Goodyear"
                                                        });
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "Kelly",
                                                            Value = "Kelly"
                                                        });
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "Michelin",
                                                            Value = "Michelin"
                                                        });
                                                        listItems.Add(new SelectListItem
                                                        {
                                                            Text = "Uniroyal",
                                                            Value = "Uniroyal"
                                                        });
                                                    }
                                                    @Html.DropDownListFor(m => m.brand, listItems, new { @class = "text-center", @style = "text-align-last: center; width:255px; height:28px", @required = "required", @id = "Brand" })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Tread</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.tread, new { htmlAttributes = new { @autocomplete = "off", @id = "TAtread", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Size</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.size, new { htmlAttributes = new { @autocomplete = "off", @id = "TAsize", @class = "text-center clear", @required = "required", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Quantity</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @{
                                                        List
                                                        <SelectListItem>
                                                        listItems1 = new List
                                                        <SelectListItem>
                                                        ();
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "Select Quantity",
                                                        });
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "1",
                                                            Value = "1"
                                                        });
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "2",
                                                            Value = "2"
                                                        });
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "3",
                                                            Value = "3"
                                                        });
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "4",
                                                            Value = "4"
                                                        });
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "5",
                                                            Value = "6"
                                                        });
                                                        listItems1.Add(new SelectListItem
                                                        {
                                                            Text = "6",
                                                            Value = "6"
                                                        });
                                                    }
                                                    @Html.DropDownListFor(m => m.quantity, listItems1, new { @class = "text", @style = "text-align-last: center; width:255px; height:28px", @required = "required", @id = "quantity" })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Current EDL</label>
                                                </div>
                                                <div class="col-md-8" style="margin-bottom:0px">
                                                    @Html.EditorFor(m => m.current_EDL, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @class = "text-center clear", @required = "required", @id = "current_EDL", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Original 32nds</label>
                                                </div>
                                                <div class="col-md-8">
                                                    @Html.EditorFor(m => m.original_32nds, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @type = "text", @class = "text-center clear", @required = "required", @id = "original_32nds", @style = "height:28px; width:255px" } })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="card shadow-sm">
                                        <div class="card-header text-center" style="font-weight:bold; padding:0px">
                                            Adjustment Type
                                        </div>
                                        <div class="card-body" style="padding:10px 20px 10px 20px">
                                            <div class="row" style="margin-bottom: 5px">
                                                <div class="col-md-3">
                                                    <h6>Default <input type="radio" id="Default" /></h6>
                                                </div>
                                                <div class="col-md-5 text-right">
                                                    <h6>Manufacturer<input type="radio" style="display: none" /> <input type="radio" id="Manufacturer" /></h6>
                                                </div>
                                                <div class="col-md-4 text-right">
                                                    <h6>Mileage <input type="radio" style="display: none" /> <input type="radio" style="display: none" /> <input type="radio" id="Mileage" /></h6>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label>Manufacture Approval #</label>
                                                </div>
                                                <div class="col-md-6">
                                                    @Html.EditorFor(m => m.manufacture_approval, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @class = "text-center clear", @id = "Manufacture_Approval", @style = "height:28px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label>Customer to Pay (%)</label>
                                                </div>
                                                <div class="col-md-6">
                                                    @Html.EditorFor(m => m.modified_adjustment_modified, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @class = "text-center clear", @id = "Modified_Adjustment_Mod", @style = "height:28px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label>Mileage Warranty (KM)</label>
                                                </div>
                                                <div class="col-md-6">
                                                    @Html.EditorFor(m => m.mileage_warranty, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @type = "text", @class = "text-center clear", @id = "Mileage_Warranty", @style = "height:28px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label style="font-size:15px">Average Tire Mileage (KM)</label>
                                                </div>
                                                <div class="col-md-6">
                                                    @Html.EditorFor(m => m.average_tire_mileage, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @type = "text", @class = "text-center clear", @id = "Average_Tire_Mileage", @style = "height:28px" } })
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label>Modified Adjustment (%)</label>
                                                </div>
                                                <div class="col-md-6">
                                                    @Html.EditorFor(m => m.modified_adjustment_mileage, new { htmlAttributes = new { @autocomplete = "off", @onkeypress = "return isNumberKey(event)", @class = "text-center clear", @id = "Modified_Adjustment", @style = "height:28px" } })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row" style="margin-bottom:0px">
                                <div class="col-md-12">
                                    <table class="table table-bordered">
                                        <thead class="thead-dark">
                                            <tr style="height:2px;">
                                                <th scope="col" style="font-size:10px; width:150px; padding:0px; margin:0px;" class="text-center">D.O.T</th>
                                                <th scope="col" style="font-size:10px; width:210px; padding:0px; margin:0px;" class="text-center">Product Code</th>
                                                <th scope="col" style="font-size:10px; width:100px; padding:0px; margin:0px;" class="text-center">32NDS</th>
                                                <th scope="col" style="font-size:10px; width:170px; padding:0px; margin:0px;" class="text-center">Mileage</th>
                                                <th scope="col" style="font-size:10px; width:150px; padding:0px; margin:0px;" class="text-center">% Worn</th>
                                                <th scope="col" style="font-size:10px; width:170px; padding:0px; margin:0px;" class="text-center">Repl Cost($)</th>
                                                <th scope="col" style="font-size:10px; width:315px; padding:0px; margin:0px;" class="text-center">Reason for removal</th>
                                            </tr>
                                        </thead>
