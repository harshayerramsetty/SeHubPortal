            <div class="modal-content">
                <div class="modal-header" style="padding:10px">
                    <h5 class="modal-title" style="color:black"><strong><span>Labour Rate </span></strong></h5>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <button class="btn btn-primary" style="padding:2px 30px; float:right; margin-bottom: 15px">
                                New Item
                            </button>

                            <table class="table table-striped table-bordered">
                                <thead style="background-color:#696a6b; color:white">
                                    <tr>
                                        <th style="font-size:10px; padding:0px; width:25%; text-align:center" scope="col">Part Number</th>
                                        <th style="font-size:10px; padding:0px; width:25%; text-align:center" scope="col">Location</th>
                                        <th style="font-size:10px; padding:0px; width:25%; text-align:center" scope="col">Rate</th>
                                        <th style="font-size:10px; padding:0px; width:25%; text-align:center" scope="col">Price</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (var item in Model.LabourRatesList)
                                    {
                                        <tr>
                                            <th style="text-align:left; font-size:13px; padding:5px; width:25%; text-align:center; font-weight:normal" scope="row">@item.part_number</th>
                                            <th style="text-align:left; font-size:13px; padding:5px; width:25%; text-align:center; font-weight:normal" scope="row">@item.loc_id</th>
                                            <th style="text-align:left; font-size:13px; padding:5px; width:25%; text-align:center; font-weight:normal" scope="row">@item.rate</th>
                                            <th style="text-align:left; font-size:13px; padding:5px; width:25%; text-align:center" scope="row">@item.unit_price</th>
                                        </tr>
                                    }


                                </tbody>
                            </table>



                        </div>
                    </div>

                </div>
                <div class="modal-footer" style="padding:8px">
                    <input type="text" readonly class="btn btn-danger" data-dismiss="modal" name="name" value="Close" style="height:25px; width:100px" />
                </div>
            </div>
        </div>
    </div>



    <script>

    </script>
</body>

</html>
