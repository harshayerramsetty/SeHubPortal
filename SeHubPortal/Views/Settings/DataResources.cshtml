                                        {
                                            <tbody>
                                                @foreach (var imp in Model.ImportHistory)
                                                {
                                                    if (imp.data_type == "CustomerList")
                                                    {
                                                        <tr>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px; width:30%">@imp.import_date.ToString("yyyy-MM-dd")</td>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px">@imp.performed_by</td>
                                                        </tr>
                                                    }
                                                }
                                            </tbody>
                                        }


                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <input type="file" name="postedFile" />
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer" style="padding:8px">
                        <button type="submit" class="btn btn-success" style="padding:1px 25px 1px 25px">Import</button>
                        <input type="text" readonly class="btn btn-danger" data-dismiss="modal" name="name" value="Close" style="height:28px; width:100px" />
                    </div>
                }
            </div>


        </div>
    </div>

    <div id="branchSalesReport" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                @using (Html.BeginForm("UploadCSVbranchSalesReport", "Settings", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {

                    <div class="modal-header" style="justify-content:center; align-items:center; padding:5px">
                        <h4 class="modal-title" style="color: black"><strong><span>Import Branch Sales</span></strong></h4>
                    </div>
                    <div class="modal-body">

                        <div class="row">
                            <div class="col-lg-12 text-center">
                                <b>Data Import History</b>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <table class="table table-striped table-bordered" style="margin-bottom:0px">
                                    <thead style="background-color:gray; color:white">
                                        <tr>
                                            <th style="padding:0px; font-size:8px; text-align:center; width:30%" scope="col">Month</th>
                                            <th style="padding:0px; font-size:8px; text-align:center; width:30%" scope="col">Import Date</th>
                                            <th style="padding:0px; font-size:8px; text-align:center" scope="col">Performed By</th>
                                        </tr>
                                    </thead>
                                </table>
                                <div class="example" style="height:222px; overflow-y:auto">

                                    <table class="table table-striped table-bordered">

                                        @if (Model.ImportHistory != null)
                                        {
                                            <tbody>
                                                @foreach (var imp in Model.ImportHistory)
                                                {
                                                    if (imp.data_type.Contains("BranchSalesReport"))
                                                    {
                                                        <tr>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px; width:30%">
                                                                @if (imp.data_type.Split(';').Count() > 1)
                                                                {
                                                                    @imp.data_type.Split(';')[1]
                                                                }
                                                            </td>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px; width:30%">@imp.import_date.ToString("yyyy-MM-dd")</td>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px">@imp.performed_by</td>
                                                        </tr>
                                                    }

                                                }
                                            </tbody>
                                        }


