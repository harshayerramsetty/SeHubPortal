                                        {
                                            <tbody>
                                                @foreach (var imp in Model.ImportHistory)
                                                {
                                                    if (imp.data_type == "PayrollDeductions")
                                                    {
                                                        <tr>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px; width:30%">@imp.import_date.ToString("yyyy-MM-dd")</td>
                                                            <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px">@imp.performed_by</td>
                                                        </tr>
                                                    }

                                                }
                                            </tbody>
                                        }


