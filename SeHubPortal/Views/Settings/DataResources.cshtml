                    <img src="https://ctasehub.blob.core.windows.net/sehub-content/labour_rate.png" style="margin-bottom:25px; width:190px;">
                    <h5 style="font-weight:bold; color:black">Labour report</h5>
                </div>
            </div>
        </div>

    </div>

    <div id="HoursBilled" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                @using (Html.BeginForm("UploadCSV", "Management", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {

                    <div class="modal-header" style="justify-content:center; align-items:center; padding:5px">
                        <h4 class="modal-title" style="color: black"><strong><span>Import Commission Report</span></strong></h4>
                    </div>
                    <div class="modal-body">

                        <div class="row">
                            <div class="col-lg-12 text-center">
                                <b>Data Import History</b>
                            </div>
                        </div>

                        <div class="row" style="margin-bottom:15px">
                            <div class="col-lg-12">
                                <table class="table table-striped table-bordered" style="margin-bottom:0px">
                                    <thead style="background-color:gray; color:white">
                                        <tr>
                                            <th style="padding:0px; font-size:8px; text-align:center; width:25%" scope="col">Payroll ID</th>
                                            <th style="padding:0px; font-size:8px; text-align:center; width:25%" scope="col">Start Date</th>
                                            <th style="padding:0px; font-size:8px; text-align:center; width:25%" scope="col">End Date</th>
                                            <th style="padding:0px; font-size:8px; text-align:center; width:45%" scope="col">Imported By</th>
                                        </tr>
                                    </thead>
                                </table>
                                <div class="example" style="height:222px; overflow-y:auto">

                                    <table class="table table-striped table-bordered">

                                        @if (Model.DataResources != null)
                                        {
                                            <tbody>
                                                @foreach (var payrol in Model.DataResources)
                                                {
                                                    <tr>
                                                        <td style="padding:0px; font-size:10px; text-align:center; padding-left:10px; width:25%">@payrol.payroll_Id</td>
                                                        <td style="padding:0px; font-size:10px; text-align:center; width:25%">@payrol.start_date.Value.ToString("yyyy-MM-dd")</td>
                                                        <td style="padding:0px; font-size:10px; text-align:center; width:25%">@payrol.end_date.Value.ToString("yyyy-MM-dd")</td>
                                                        <td style="padding:0px; font-size:10px; text-align:center; width:45%">Blackwood, Jordan</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        }


                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <input type="file" name="postedFile" />
                            </div>
                        </div>

                        <div class="row" style="margin-top:10px">
                            <div class="col-lg-12">
                                <input type="checkbox" name="name" value="" />
                                Overwrite existing
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer" style="padding:8px">
                        <button type="submit" class="btn btn-success" style="padding:1px 25px 1px 25px">Import</button>
                        <button type="button" data-dismiss="modal" class="btn btn-danger" style="padding:1px 25px 1px 25px">Cancel</button>
                    </div>
                }
            </div>


        </div>
    </div>

