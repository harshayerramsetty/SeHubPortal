                                </div>
                                <div style="flex:6">
                                    <input autocomplete="off" onkeypress="return isNumberKey(event)" style="width:100%" onfocusout="check_required_fields_tire(this.id.split('right_tire')[1].split('_pressure')[0])" id="@pressure" type="text" name="name" value="" />
                                </div>
                            </div>

                            <div style="display:flex; margin-bottom:0.6vw">
                                <div style="flex:6">
                                    <label style="font-size:10px"><i>Unable to confirm PSI</i></label>
                                </div>
                                <div style="flex:6">
                                    <input type="checkbox" onclick=" let id = this.id.split('right_tire')[1].split('_pressure_check')[0];
                                                                             if ($(this).is(':checked')) {
                                                                                 $('#right_tire' + id + '_pressure').val('n/a')
                                                                                 $('#right_tire' + id + '_pressure').prop('readonly', true);
                                                                                 $('#right_tire' + id + '_pressure').css('background-color', '#f8f8f8');
                                                                             }
                                                                             else {
                                                                                 $('#right_tire' + id + '_pressure').val('')
                                                                                 $('#right_tire' + id + '_pressure').prop('readonly', false);
                                                                                 $('#right_tire' + id + '_pressure').css('background-color', 'white');
                                                                                 $('#right_tire' + id + '_pressure').focus();
                                                                             }
                                                 " style="        width: 1.5vw;
        height: 1.5vw" id="@pressure_check" name="name" value="" />
