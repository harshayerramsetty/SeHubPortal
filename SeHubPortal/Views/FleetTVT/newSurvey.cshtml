            @for (int i = 1; i <= 20; i++)
            {

                string pressure = "right_tire" + i + "_pressure";
                string pressure_check = "right_tire" + i + "_pressure_check";
                string depth = "right_tire" + i + "_depth";
                string wear = "right_tire" + i + "_wear";
                string brand = "right_tire" + i + "_brand";
                string size = "right_tire" + i + "_size";
                string modelid = "right_tire" + i + "_model";
                string comments = "right_tire" + i + "_comments";

                string brand_freight = brand + "_freight";
                string size_freight = size + "_freight";
                string modelid_freight = modelid + "_freight";

                string save = "right_tire" + i + "_save";
                string cancel = "right_tire" + i + "_cancel";

                string brand_other = "t" + i + "b";
                string size_other = "t" + i + "s";

                <div class="card @i" style="display:none; width:100%; height:90vh">
                    <div class="card-header" style="padding: 0.2vw 1vw">
