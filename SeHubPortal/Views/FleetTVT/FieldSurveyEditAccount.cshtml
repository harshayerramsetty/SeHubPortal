    <script>


        $('#dateExpiredUnitSurvey').change(function () {
            $("#Step2").css('visibility', 'visible');

            deselectEverything()

            var d = new Date($('#dateExpiredUnitSurvey').val());
            d = addDays(d, 1);
            date = d.toString();

            var shortDay = date.substring(0, 3);
            var longDay = "";

            if (shortDay == "Mon") {
                longDay = "Monday"
            }
            else if (shortDay == "Tue")
            {
                longDay = "Tuesday"
            }
            else if (shortDay == "Wed")
            {
                longDay = "Wednesday"
            }
            else if (shortDay == "Thu")
            {
                longDay = "Thursday"
            }
            else if (shortDay == "Fri")
            {
                longDay = "Friday"
            }
            else if (shortDay == "Sat")
            {
                longDay = "Saturday"
            }
            else if (shortDay == "Sun")
            {
                longDay = "Sunday"
            }

            var month = date.substring(4, 7);

            var dateNum = date.substring(8, 10);

            var yearNum = date.substring(11, 15);


            $("#fullDateEquipmentSurvey").val(longDay + " - " + month + " " + dateNum + ", " + yearNum);

        });



        $.ajax({
                url: '@Url.Action("PullEmployeesChangeLocation", "FleetTVT")',
                data: { value: @Model.Location },
                type: "POST",
                success: function (data) {
                    var s = '<option value="-1">Select Technician</option>';
                    for (var i = 0; i < data.length; i++) {
                        s += '<option value="' + data[i].value + '">' + data[i].text + '</option>';
                        console.log('<option value="' + data[i].value + '">' + data[i].text + '</option>');
                    }
                    $("#EmpsDropdown").html(s);
                }
        });



        $('#Next').click(function(e){           
            $("#profile-tab").removeClass("disabled");            
            e.preventDefault();
            $('#myTab a:last').tab('show');   
        })



        function check_requiredFileds_unit() {

            var actualCount = 4;
            var count = 0;

            $('#UnitReqDiv :input').each(function(){
                if(this.value.length > 0){ count++; };
            });


            

            if ($('#mileageInput').is(':disabled')) {
                if ($("#dateExpiredUnitSurvey").val() && $("#surveyLocationUnit").val() && $("#EmpsDropdown").val() && $("#EmpsDropdown option:selected").text() != "Select Technician") {

                    $('#Next').css('visibility', 'visible');

                    $("#profile-tab").removeClass("disabled");

                    if ($('.tireProgress').text() == "100%") {
                        $('#submiSurvey').css('visibility', 'visible');                    
                    }

                }
                else {

                    $('#submiSurvey').css('visibility', 'hidden');
                    $('#Next').css('visibility', 'hidden');
                    $("#profile-tab").addClass("disabled");
                }
            }
            else {
                if ($("#mileageInput").val() && $("#dateExpiredUnitSurvey").val() && $("#surveyLocationUnit").val() && $("#EmpsDropdown").val() && $("#EmpsDropdown option:selected").text() != "Select Technician") {

                    $('#Next').css('visibility', 'visible');

                    $("#profile-tab").removeClass("disabled");

                    if ($('.tireProgress').text() == "100%") {
                        $('#submiSurvey').css('visibility', 'visible');                    
                    }

                }
                else {

                    $('#submiSurvey').css('visibility', 'hidden');
                    $('#Next').css('visibility', 'hidden');
                    $("#profile-tab").addClass("disabled");
                }
            }



            if ($("#EmpsDropdown option:selected").text() == "Select Technician") {
                count--;
            }


            var unitPercentage = (count / actualCount) * 100;

            if ($('#mileageInput').is(':disabled')) {
                var unitPercentage = (count / actualCount) * 100;
            }
            else {
                var unitPercentage = (count / actualCount) * 100 - 25;
            }



            var percentageStr = unitPercentage.toString();

            percentageStr = percentageStr + '%'


            $('.unitProgress').css('width', percentageStr).text(percentageStr);

        }

        function changeLocation(value1) {

            $.ajax({
                url: '@Url.Action("PullEmployeesChangeLocation", "FleetTVT")',
                data: { value: value1 },
                type: "POST",
                success: function (data) {
                    var s = '<option value="-1">Select Technician</option>';
                    for (var i = 0; i < data.length; i++) {
                        s += '<option value="' + data[i].value + '">' + data[i].text + '</option>';
                        console.log('<option value="' + data[i].value + '">' + data[i].text + '</option>');
                    }
                    $("#EmpsDropdown").html(s);
                }
            });
        }


        datepicker.max = new Date().toISOString().split("T")[0];


        function addDays(date, days) {
          var result = new Date(date);
          result.setDate(result.getDate() + days);
          return result;
        }

        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode != 46 && charCode > 31
                && (charCode < 48 || charCode > 57))
                return false;
            return true;
        }


        $('#unitTab').click(function () {
            deselectEverything();
        });

        $('#clearEquipmentSurvey').click(function () {
            $('#mileageInput').val("");
            $('#surveyLocationUnit').val("");
