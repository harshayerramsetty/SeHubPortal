        .nav-tabs > li > a {
            /* adjust padding for height*/
            padding-top: 1px;
            padding-bottom: 1px;
        }

        .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a {
            margin-top: 1px;
        }

        .nav-tabs > li {
            margin-bottom: 0px;
        }

            .nav-tabs > li.active {
                margin-bottom: -1px;
            }
