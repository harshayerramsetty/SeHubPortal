        <style>
            .hex {
              position: absolute;
              width: 150px;
              height: 240px;
              border-radius: 20px;
              background: white;
              transition: opacity .5s;
            }

            .hex:before,
            .hex:after {
              position: absolute;
              width: inherit;
              height: inherit;
              border-radius: inherit;
              background: inherit;
              content: '';
            }

            .hex:before {
              -webkit-transform: rotate(60deg);
              transform: rotate(60deg);
            }

            .hex:after {
              -webkit-transform: rotate(-60deg);
              transform: rotate(-60deg);
            }

            body{
                padding:0;
                margin:0
            }

        </style>

