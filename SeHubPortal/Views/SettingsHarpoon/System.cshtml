
@{
    ViewBag.Title = "System";
    Layout = "~/Views/Shared/_Settings_Harpoon.cshtml";
}

@model SeHubPortal.ViewModel.AccountHarpoonSettingsViewModel


<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    @*<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>*@

    @if (Model.client.employees_page.Value)
    {
        <style>
            #ManagementClicked {
                display: block !important;
            }
        </style>
    }
    @if (Model.client.fleet_page.Value)
    {
        <style>
            #VehicleClicked {
                display: block !important;
            }
        </style>
    }

    <style>

        #SettingsClicked {
            display: block !important;
        }

        #ManagementClicked {
            display: block !important;
        }

        .example::-webkit-scrollbar {
            display: none;
        }

        .example {
            -ms-overflow-style: none; /* IE and Edge */
            scrollbar-width: none; /* Firefox */
        }

        #SystemLink {
            background-color: #f0f2f5;
            color: black;
            font-weight: 600;
        }

        #SettingsClicked {
            background-color: #f0f2f5;
            color: black;
            font-weight: 600;
        }

        .settingsBtn {
            font-weight: bold;
        }

        #SystemLink {
            color: black;
            font-weight: 700;
            background-color: #f0f2f5;
        }

        body {
            background-color: #f0f2f5;
            padding-right: 0 !important;
        }

        .heading {
            margin: 10px 0px 0px 0px;
        }

            .heading h2 {
                margin-top: 8px;
            }

        .card-header {
            background-color: #cde8ca;
        }

            .card-header h6 {
                color: black;
            }

        .imageClass {
            width: 100%;
            height: 80%;
            border-radius: 50%;
        }

        .card {
            border: 1px solid #ededed;
            margin-bottom: 30px;
            border-radius: 10px;
        }

        .card {
            position: relative;
            display: -ms-flexbox;
            display: flex;
            height: auto;
            -ms-flex-direction: column;
            flex-direction: column;
            min-width: 0;
            word-wrap: break-word;
            background-color: #fff;
            background-clip: border-box;
            border: 1px solid rgba(0,0,0,.125);
            border-radius: .25rem;
        }

        p {
            font-size: 14px;
        }

        #MatchedStaffLocID {
            height: 30px;
            width: 100px;
        }
    </style>
</head>

<body>

    @{
        string base64ProfilePic = "";
        if (Model.client.client_logo is null)
        {
            base64ProfilePic = "";
        }
        else
        {
            base64ProfilePic = "data:image/png;base64," + Convert.ToBase64String(Model.client.client_logo);
        }
    }

    <div class="row heading">

        <div class="col" style="margin:0px 5px 15px 15px; padding:10px; background-color:white">

            <div style="display:flex; justify-content:space-between; align-items:center">
                <div>
                    <a href="@Url.Action("System", "SettingsHarpoon")">Settings </a>/ Account
                </div>
                
            </div>
        </div>
        <button type="button" id="clickThis" class="btn" style="margin-bottom:14px;">
            <i class="fa fa-bars" style="font-size:25px"></i>
            <span class="sr-only">Toggle Menu</span>
        </button>
    </div>

    <div class="row" style="padding:0px 15px 0px 15px">
        <div class="col-lg-3" style="padding-right:0px">
            <div class="card">
                <div class="card-body">
                    <div class="row" style="margin:0px 0px 10px 0px; padding:0px">
                        <div class="col-lg-12 text-center" style="border:1px solid dimgray; padding:0px 20px 0px 20px">
                            <div class="row">
                                <img class="imageClass" style="border-radius:0px; width:90%; height:315px; margin-left:auto; margin-right:auto; padding:0px" alt="profilePic" src="@base64ProfilePic" />
                            </div>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:10px">
                        <div class="col-lg-4 text-right" style="font-size:12px; font-weight:bold; color:dimgray; padding-right:0px">
                            Client ID
                        </div>
                        <div class="col-lg-8">
                            <input readonly type="text" id="client_id" style="font-weight:bold; width:100%" name="name" value="@Model.client.client_id" />
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:10px">
                        <div class="col-lg-4 text-right" style="font-size:12px; font-weight:bold; color:dimgray; font-size:12px; padding-right:0px">
                            Name
                        </div>
                        <div class="col-lg-8">
                            <input readonly type="text" style="font-weight:bold; width:100%" name="name" value="@Model.client.client_name" />
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:10px">
                        <div class="col-lg-4 text-right" style="font-size:12px; font-weight:bold; color:dimgray; padding-right:0px">
                            Address
                        </div>
                        <div class="col-lg-8" style="overflow-y:auto">
                            <div style="font-weight:bold; padding-left:5px; height:130px; border:1px solid dimgray">
                                @Model.client.client_address1
                                <br />
                                @Model.client.client_city
                                <br />
                                @Model.client.client_province @Model.client.client_postal @Model.client.client_country

                            </div>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:10px">
                        <div class="col-lg-4 text-right" style="font-size:12px; font-weight:bold; color:dimgray; padding-right:0px">
                            Phone
                        </div>
                        <div class="col-lg-8">
                            <input readonly type="text" style="font-weight:bold; width:100%" name="name" value="@Model.client.client_phone" />
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:10px">
                        <div class="col-lg-4 text-right" style="font-size:12px; font-weight:bold; color:dimgray; padding-right:0px">
                            Fax
                        </div>
                        <div class="col-lg-8">
                            <input readonly type="text" style="font-weight:bold; width:100%" name="name" value="@Model.client.client_fax" />
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:10px">
                        <div class="col-lg-4 text-right" style="font-size:12px; font-weight:bold; color:dimgray; padding-right:0px">
                            Website
                        </div>
                        <div class="col-lg-8">
                            <input readonly type="text" style="font-weight:bold; width:100%" name="name" value="@Model.client.client_website" />
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:0px;">
                        <div class="col-lg-4">

                        </div>
                        <div class="col-lg-8">
                            <span>
                                <button type="button" value="@Model.client.client_id;@Model.client.client_name;@Model.client.client_address1;@Model.client.client_address2;@Model.client.client_phone;@Model.client.client_fax;@Model.client.client_website" onclick="edit(this.value)" data-toggle="modal" data-target="#EditClient" style="background-color: transparent;border: none;outline: none;">
                                    <i class="fa fa-edit" style="font-size:25px;color:black;float:right"></i>
                                </button>
                            </span>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <div class="col-lg-9" style="padding:0px 30px 0px 30px">
            <div class="row">
                <div class="card">

                    @using (Html.BeginForm("SaveAccountSettings", "SettingsHarpoon", FormMethod.Post))
                    {
                        <div class="card-body example" style="height:737px; overflow-y:auto; width:100%">

                            @Html.HiddenFor(m => m.client.client_id)
                            <div class="row" id="saveButton" style="display:none; padding:0px 15px 15px 15px">
                                <div class="col-lg-12 text-center" style="background-color: #fdcf16; padding: 5px 0px 5px 0px">
                                    <button type="submit" style="margin:5px; padding:3px 30px 3px 30px" class="btn btn-success">Save</button>
                                    <button type="button" onclick="location.reload()" style="margin:5px; padding:3px 20px 3px 20px" class="btn btn-danger">Cancel</button>
                                </div>
                            </div>


                            <div class="row">
                                <div class="col-lg-6" style="padding-right:0px">
                                    <div class="row" style="margin-bottom:10px">
                                        <div class="col-lg-2" style="padding: 0px 0px 0px 30px">
                                            <div class="row">
                                                @if (Model.autoClockOut)
                                                {
                                                    <label id="Toggle1_off" style="margin-right:10px">
                                                        OFF
                                                    </label>
                                                }
                                                else
                                                {
                                                    <label id="Toggle1_off" style="margin-right:10px; font-weight:bold">
                                                        OFF
                                                    </label>
                                                }

                                            <div class="custom-control custom-switch">
                                                @Html.EditorFor(m => m.autoClockOut, new { htmlAttributes = new { @class = "custom-control-input", @id = "Toggle1", @type = "checkbox" } })

                                                @if (Model.autoClockOut)
                                                {
                                                    <label id="Toggle1_on" class="custom-control-label" for="Toggle1" style="font-weight:bold">
                                                        ON
                                                    </label>
                                                }
                                                else
                                                {
                                                    <label id="Toggle1_on" class="custom-control-label" for="Toggle1">
                                                        ON
                                                    </label>
                                                }


                                            </div>

                                            </div>
                                        </div>
                                        <div class="col-lg-10" style="padding-left:50px">
                                            <h5><u><b>Custom Auto Clock-Out Procedure</b></u></h5>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:10px">
                                        <div class="col-lg-2"></div>
                                        <div class="col-lg-10 text-right" style="padding-left:50px; padding-right:12px">
                                            <div class="row">
                                                <div class="col-lg-3 text-left" style="padding-right:0px">
                                                    <b>Adjustment</b>
                                                </div>
                                                <div class="col-lg-9 text-left">

                                                    @if (Model.autoClockOut)
                                                    {
                                                        @Html.EditorFor(m => m.adjustment, new { htmlAttributes = new { @onfocusout = "MaxVal(this.value)", @onchange = "saveRowDisplay()", @style = "font-size:20px; font-weight:bold; text-align:center; width:110px; height:40px", @id = "appt-time", @type = "number", @name = "appt-time", @min = "0", @max = "30", @placeholder = "00" } })
                                                    }
                                                    else
                                                    {
                                                        @Html.EditorFor(m => m.adjustment, new { htmlAttributes = new { @onfocusout = "MaxVal(this.value)", @onchange = "saveRowDisplay()", @readonly = true, @style = "font-size:20px; font-weight:bold; text-align:center; width:110px; height:40px", @id = "appt-time", @type = "number", @name = "appt-time", @min = "0", @max = "30", @placeholder = "00" } })
                                                    }

                                                    <label style="font-weight:bold; color:dimgray">minutes</label>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row" style="margin-bottom:10px">
                                        <div class="col-lg-2"></div>
                                        <div class="col-lg-10 text-right" style="padding-left:50px; padding-right:0px">
                                            <div class="row">
                                                <div class="col-lg-3 text-left" style="padding-right:0px">
                                                    <b>Default (OFF)</b>
                                                </div>
                                                <div class="col-lg-9 text-left" style="font-size:15px">
                                                    Auto clock-out time set to closing time of the day
                                                </div>
                                            </div>

                                        </div>

                                    </div>

                                    <div class="row" style="margin-bottom:10px">
                                        <div class="col-lg-2"></div>
                                        <div class="col-lg-10 text-right" style="padding-left:50px; padding-right:0px">
                                            <div class="row">
                                                <div class="col-lg-3 text-left" style="padding-right:0px">
                                                    <b>Custom (ON)</b>
                                                </div>
                                                <div class="col-lg-9 text-left" style="font-size:15px">
                                                    Auto clock-out time set to closing time of the day minus specified value above. Strategy used to encourage clock-out via key fob or manual code
                                                </div>
                                            </div>

                                        </div>

                                    </div>

                                    <div class="row" style="margin-bottom:10px">
                                        <div class="col-lg-2"></div>
                                        <div class="col-lg-10 text-left" style="font-size:15px; padding-left:50px; padding-right:0px">
                                            <b><i>Auto clock-out time for <u>non-operating days</u> set to clock-in time</i></b>
                                        </div>

                                    </div>

                                </div>
                                <div class="col-lg-6" style="padding: 0px 15px 15px 15px; font-size: 13px">
                                    <div style="padding:0px 15px 0px 15px; border:2px solid dimgray; border-radius:15px">
                                        <div class="row" style="margin-top:15px; margin-bottom:10px">
                                            <div class="col-lg-1"></div>
                                            <div class="col-lg-2">
                                                <b><u>Schedule</u></b>
                                            </div>
                                            <div class="col-lg-2"></div>
                                            <div class="col-lg-2"></div>
                                            <div class="col-lg-2"></div>
                                            <div class="col-lg-3 text-left" style="color:#007bff">
                                                <b><u>Auto Clock-Out</u></b>
                                            </div>
                                        </div>


                                        <div class="row">
                                            <div class="col-lg-2 text-right" style="padding-right:0px">
                                                <label style="margin-right:10px">
                                                    <b>Sunday</b>
                                                </label>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="row">
                                                    <div class="col-lg-12 text-left" style="padding-left:35px">
                                                        <div class="custom-control custom-switch">
                                                            @Html.EditorFor(m => m.sundayOpen, new { htmlAttributes = new { @checked = true, @class = "custom-control-input", @id = "ToggleSunday", @type = "checkbox" } })
                                                            <label class="custom-control-label" for="ToggleSunday">
                                                                <b class="ToggleSunday">
                                                                    @if (Model.sundayOpen)
                                                                    {
                                                                        <span>
                                                                            Open
                                                                        </span>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>
                                                                            Closed
                                                                        </span>
                                                                    }
                                                                </b>
                                                            </label>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.sundayStart, new { htmlAttributes = new { @class = "sundayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time", @id = "sunStart" } })
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.sundayEnd, new { htmlAttributes = new { @onfocusout = "sunen()", @class = "sundayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time", @id = "sunEnd" } })
                                            </div>
                                            <script>
                                                function sunen() {
                                                    timeCalculation($('#sunEnd').val(), $('#appt-time').val(), "sunAuto")
                                                }
                                            </script>
                                            <div class="col-lg-3 text-center">
                                                <input class="sundayTime" readonly id="sunAuto" style="color:#007bff; border:none; outline:none; margin-left:auto; margin-right:auto; font-size:14px; font-weight:bold; text-align:center; width:100px" type="time" name="name" value="" />
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg-2 text-right" style="padding-right:0px">
                                                <label style="margin-right:10px">
                                                    <b>Monday</b>
                                                </label>
                                            </div>
                                            <div class="col-lg-3 text-right">
                                                <div class="row">
                                                    <div class="col-lg-12 text-left" style="padding-left:35px">
                                                        <div class="custom-control custom-switch">
                                                            @Html.EditorFor(m => m.monOpen, new { htmlAttributes = new { @checked = true, @class = "custom-control-input", @id = "ToggleMonday", @type = "checkbox" } })
                                                            <label class="custom-control-label" for="ToggleMonday">
                                                                <b class="ToggleMonday">
                                                                    @if (Model.monOpen)
                                                                    {
                                                                        <span>
                                                                            Open
                                                                        </span>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>
                                                                            Closed
                                                                        </span>
                                                                    }
                                                                </b>
                                                            </label>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.monStart, new { htmlAttributes = new { @class = "mondayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time" } })
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.monEnd, new { htmlAttributes = new { @onfocusout = "monen()", @class = "mondayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time", @id = "monEnd" } })
                                            </div>
                                            <script>
                                                function monen() {
                                                    timeCalculation($('#monEnd').val(), $('#appt-time').val(), "monAuto")
                                                }
                                            </script>
                                            <div class="col-lg-3 text-center">
                                                <input class="mondayTime" readonly id="monAuto" style="color:#007bff; border:none; outline:none; margin-left:auto; margin-right:auto; font-size:14px; font-weight:bold; text-align:center; width:100px" type="time" name="name" value="" />
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg-2 text-right" style="padding-right:0px">
                                                <label style="margin-right:10px">
                                                    <b>Tuesday</b>
                                                </label>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="row">
                                                    <div class="col-lg-12 text-left" style="padding-left:35px">
                                                        <div class="custom-control custom-switch">
                                                            @Html.EditorFor(m => m.TueOpen, new { htmlAttributes = new { @checked = true, @class = "custom-control-input", @id = "ToggleTuesday", @type = "checkbox" } })

                                                            <label class="custom-control-label" for="ToggleTuesday">
                                                                <b class="ToggleTuesday">
                                                                    @if (Model.TueOpen)
                                                                    {
                                                                        <span>
                                                                            Open
                                                                        </span>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>
                                                                            Closed
                                                                        </span>
                                                                    }
                                                                </b>
                                                            </label>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.TueStart, new { htmlAttributes = new { @class = "tuesdayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time" } })
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.TueEnd, new { htmlAttributes = new { @onfocusout = "tueen()", @class = "tuesdayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time", @id = "tueEnd" } })
                                            </div>
                                            <script>
                                                function tueen() {
                                                    timeCalculation($('#tueEnd').val(), $('#appt-time').val(), "tueAuto")
                                                }
                                            </script>
                                            <div class="col-lg-3 text-center">
                                                <input class="tuesdayTime" readonly id="tueAuto" style="color:#007bff; border:none; outline:none; margin-left:auto; margin-right:auto; font-size:14px; font-weight:bold; text-align:center; width:100px" type="time" name="name" value="" />
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg-2 text-right" style="padding-right:0px">
                                                <label style="margin-right:10px">
                                                    <b>Wednesday</b>
                                                </label>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="row">
                                                    <div class="col-lg-12 text-left" style="padding-left:35px">
                                                        <div class="custom-control custom-switch">
                                                            @Html.EditorFor(m => m.WedOpen, new { htmlAttributes = new { @checked = true, @class = "custom-control-input", @id = "ToggleWednesday", @type = "checkbox" } })
                                                            <label class="custom-control-label" for="ToggleWednesday">
                                                                <b class="ToggleWednesday">
                                                                    @if (Model.WedOpen)
                                                                    {
                                                                        <span>
                                                                            Open
                                                                        </span>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>
                                                                            Closed
                                                                        </span>
                                                                    }
                                                                </b>
                                                            </label>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.WedStart, new { htmlAttributes = new { @class = "wednesdayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time" } })
                                            </div>
                                            <div class="col-lg-2 text-center">
                                                @Html.EditorFor(m => m.WedEnd, new { htmlAttributes = new { @onfocusout = "weden()", @class = "wednesdayTime", @onchange = "saveRowDisplay()", @style = "font-size:10px; font-weight:bold; text-align:center; width:80px", @type = "time", @id = "wedEnd" } })
                                            </div>
                                            <script>
                                                function weden() {
                                                    timeCalculation($('#wedEnd').val(), $('#appt-time').val(), "wedAuto")
                                                }
                                            </script>
                                            <div class="col-lg-3 text-center">
                                                <input class="wednesdayTime" readonly id="wedAuto" style="color:#007bff; border:none; outline:none; margin-left:auto; margin-right:auto; font-size:14px; font-weight:bold; text-align:center; width:100px" type="time" name="name" value="" />
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg-2 text-right" style="padding-right:0px">
                                                <label style="margin-right:10px">
                                                    <b>Thursday</b>
                                                </label>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="row">
                                                    <div class="col-lg-12 text-left" style="padding-left:35px">
                                                        <div class="custom-control custom-switch">
                                                            @Html.EditorFor(m => m.ThuOpen, new { htmlAttributes = new { @checked = true, @class = "custom-control-input", @id = "ToggleThursday", @type = "checkbox" } })
                                                            <label class="custom-control-label" for="ToggleThursday">
                                                                <b class="ToggleThursday">
                                                                    @if (Model.ThuOpen)
                                                                    {
                                                                        <span>
                                                                            Open
                                                                        </span>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>
                                                                            Closed
                                                                        </span>
                                                                    }
                                                                </b>
                                                            </label>
                                                        </div>
