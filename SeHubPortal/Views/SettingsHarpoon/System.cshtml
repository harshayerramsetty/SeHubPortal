        function timeCalculation(day, adj, id) {
            if (parseInt(day.substring(3, 5)) - parseInt(adj) >= 0) {

                if (String(parseInt(day.substring(3, 5)) - parseInt(adj)).length <= 1) {
                    $("#" + id).val(String(day.substring(0, 3)) + makeDouble(String(parseInt(day.substring(3, 5)) - parseInt(adj))));
                }
                else {
                    $("#" + id).val(String(day.substring(0, 3)) + String(parseInt(day.substring(3, 5)) - parseInt(adj)));
                }


            }
            else {
                $("#" + id).val(String(parseInt(day.substring(0, 3)) - 1) + ":" + String(60 + parseInt(day.substring(3, 5)) - parseInt(adj)))
            }
        }

        function makeDouble(num) {
            var formattedNumber = ("0" + num).slice(-2);
            return formattedNumber
