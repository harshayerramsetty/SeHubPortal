    <script>
        $(".serialDot").focusout(function () {
            var code = $(this).val().substr(0, 2)
            var idis = $(this).attr('id')
            var lastChar = idis[idis.length - 1];

            

            $.ajax({
                url: '@Url.Action("getDropdownOptions", "Tools")',
                data: { value: code },
                type: 'POST',
                success: function (data) {
                    if (data != " ") {
                        brandID = '#brand' + lastChar
                        $(brandID).empty()

                        const dropdownArray = data.split(";");
                        dropdownArray.forEach(function (item) {
                            item = item.replace(/\s+/g, '');
                            $(brandID).append($("<option></option>").attr("value",item).text(item));
                        });
                    }
                }
            });
            
            $.ajax({
                url: '@Url.Action("CheckPlantCodes", "Tools")',
                data: { value: code },
                type: 'POST',
                success: function (data) {

                    if (data != " ") {
                        brandID = '#brand' + lastChar
                        $(brandID).val(data)
                    }

                    
                    //
                }
            });

            var sizecode = $(this).val().substr(2, 2)

            $.ajax({
                url: '@Url.Action("CheckSizeCodes", "Tools")',
                data: { value: sizecode },
                type: 'POST',
                success: function (data) {
                    if (data != " ") {
                        sizeID = '#casing' + lastChar
                        $(sizeID).val(data)
                    }
                    
                }
            });

        });
    </script>

