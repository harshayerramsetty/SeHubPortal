            });

        }

        function SaveToTempShip(val) {
            $('.commonShipClass').css('bordor', '2px solid dimgray').css('background-color', 'transparent').css('color', 'dimgray')
            $('#' + val).css('bordor', 'none').css('background-color', '#007bff').css('color', 'white')

            $('.commonShipText').css('font-weight', 'normal')
            $('.' + val).css('font-weight', 'bold')


            $('#tempShip').val(val)
            $('#tempShip').css("display", "block")
        }

        function SaveToTempBrand(val) {
            $('.commonBrandClass').css('border', '2px solid dimgray')
            $('#' + val).css('border', '10px solid #007bff')
            $('#tempBrand').val(val)
            $('#tempBrand').css("display", "block")
        }

        function SaveToTempTread(val) {
            $('.commonTreadClass').css('border', '2px solid dimgray').css('color', 'dimgray').css('background-color', 'transparent')
            $('#' + val).css('background-color', '#007bff').css('color', 'white').css('border', 'none')
            $('#tempTread').val(val)
            $('#tempTread').css("display", "block")
        }

        function SaveToTempStatus(val) {
            $('.commonFailureCodes').css("font-weight", "normal")
            $('.' + val).css("font-weight", "bold")
            $('#tempStatus').val(val)
            $('#tempStatus').css("display", "block")
        }

        function SaveToTemp(val) {
            $('.commonSizeClass').css('border', '2px solid dimgray').css('color', 'dimgray').css('background-color', 'transparent')
            $('#tempSize').val(val)
            $('#tempSize').css("display", "block")
        }
        function changeBackground(e) {
            $(e).css('background-color', '#007bff').css('color', 'white').css('border', 'none')
        }

        function StatusInfo(val) {
            $('#statusInput').val(val);
            $("#SubmitButton").removeAttr('disabled').removeClass("btn-secondary").addClass("btn-success");
        }

        function SaveTread(val) {
            $('#treadInput').val(val);
            $('#okTread').css("display", "block");
            $("#SubmitButton").removeAttr('disabled').removeClass("btn-secondary").addClass("btn-success");
        }

        function SaveSize(val) {
            $('#sizeInput').val(val);
            $("#SubmitButton").removeAttr('disabled').removeClass("btn-secondary").addClass("btn-success");
        }

        function SaveShipto(val) {
            $('#shiptoInput').val(val);
            $('#okShip').css("display", "block");
            $("#SubmitButton").removeAttr('disabled').removeClass("btn-secondary").addClass("btn-success");
        }

        function SaveBrand(val) {
            $('#brandInput').val(val);
            $('#okBrand').css("display", "block");
            $("#SubmitButton").removeAttr('disabled').removeClass("btn-secondary").addClass("btn-success");

        }

        $(document).ready(function () {
            function getUrlVars() {
                var vars = [], hash;
                var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                for (var i = 0; i < hashes.length; i++) {
                    hash = hashes[i].split('=');
                    vars.push(hash[0]);
                    vars[hash[0]] = hash[1];
                }
                return vars;
            }

            var acknowledgment = getUrlVars()["pane"];

            if (acknowledgment == "IncorrectBarcode") {
                $('#IncorrectBarcode').modal();
            }
        })
    </script>

    <script>
        function appendBarcode(val) {

            if (val == "10") {
                $('#ndt_barcode').val("");
                $('#ExistsIcon').css("display", "none")
                $('#DoesntExixtIcon').css("display", "none")
                $('#keyboardSubmit').prop({ disabled: true })
            }
            else if (val == "DEL") {
                let barcodeDel = $('#ndt_barcode').val();
                $('#ndt_barcode').val(barcodeDel.slice(0, -1));
                $('#ExistsIcon').css("display", "none")
                $('#DoesntExixtIcon').css("display", "none")
                $('#keyboardSubmit').prop({ disabled: true })
            }
            else {

                    if ($('#ndt_barcode').val().length < 8) {
                        let barcode = $('#ndt_barcode').val();
                        $('#ndt_barcode').val(barcode + val);
                    }
                    if ($('#ndt_barcode').val().length == 8) {

                        $.ajax({
                                    url: '@Url.Action("validateBarcode", "Tools")',
                                    data: { value: $('#ndt_barcode').val() },
                                    type: 'POST',
                                    success: function (data) {

                                        if (data == "Exists") {

                                            $('#ExistsIcon').css("display", "block")
                                            $('#DoesntExixtIcon').css("display", "none")
                                            $('#keyboardSubmit').prop({ disabled: false })

                                        }
                                        else {
                                            $('#ExistsIcon').css("display", "none")
                                            $('#DoesntExixtIcon').css("display", "block")
                                            $('#keyboardSubmit').prop({ disabled: true })
                                        }

                                    }
                                });


                }
                    else {
                                $('#ExistsIcon').css("display", "none")
                                $('#DoesntExixtIcon').css("display", "none")
                        }
                 }
        }

    </script>



