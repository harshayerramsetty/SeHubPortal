@model SeHubPortal.ViewModel.PlantViewModel
@{
    ViewBag.Title = "BufferBuilder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap4.min.js"></script>


    @if (Model.SehubAccess.tools > 0)
    {
        <style>
            #toolsClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.management > 0)
    {
        <style>
            #ManagementClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.settings > 0)
    {
        <style>
            #SettingsClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.payroll > 0)
    {
        <style>
            #PayrollClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.main > 0)
    {
        <style>
            #mainClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.fleetTVT > 0)
    {
        <style>
            #FleetTVTClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.treadTracker > 0)
    {
        <style>
            #treadTrackereClicked {
                display: block !important;
            }
        </style>
    }

    @if (Model.SehubAccess.library_access > 0)
    {
        <style>
            #libraryClicked {
                display: block !important;
            }
        </style>
    }

    <style>

        #DashboardLink {
            display: block !important;
        }

        #PlantClicked {
            display: block !important;
        }


        #PlantBtn {
            font-weight: bold;
        }

        #PlantClicked {
            background-color: #f0f2f5;
            color: black;
            font-weight: 600;
        }

        #DashboardLink {
            color: black;
            font-weight: 700;
            background-color: #f0f2f5;
        }

        body {
            background-color: #f0f2f5;
            padding: 0px !important;
            margin: 0px !important;
        }

        .card-header {
            background-color: #cde8ca;
        }

            .card-header h6 {
                color: black;
            }

        .example::-webkit-scrollbar {
            display: none;
        }

        .example {
            -ms-overflow-style: none; /* IE and Edge */
            scrollbar-width: none; /* Firefox */
        }

        #StatusModal {
            padding-right: 20%
        }

        #TreadModal {
            padding-right: 15%
        }

        #SizesModal {
            padding-right: 10%
        }

        #ShiptoModal {
            padding-right: 4%
        }

        #BrandModel {
            padding-right: 15%
        }

        #Keyboard {
            padding-top: 6%
        }


    </style>
</head>
<body style="overflow-y: hidden; overflow-x: hidden">

    <div class="row">
        <div class="col-md-12">
            <div class="row" style="padding:10px 30px 10px 30px">
                <div class="col-md-4" style="padding:10px; background-color:white;">
                    <a href="@Url.Action("Plant", "Retread")">Retread Plant</a> / @ViewData["Station"]
                </div>
                <div class="col-md-4 text-center" style="padding:10px; background-color:white;">
                    @if (Model.paneType != "barcode")
                    {
                        <button id="keyboardbtn" data-target="#Keyboard" data-toggle="modal" class="btn btn-primary" style="padding:2px 30px 2px 30px">
                            <b>Keyboard</b>
                        </button>
                    }
                </div>
                <div class="col-md-4" style="padding:10px; background-color:white;">

                    @if (Model.paneType == "barcode")
                    {
                        <button onclick="location.href='@Url.Action("BufferBuilder", "Retread")';" style="float:right; background-color:transparent; outline:none; border:none">
                            <img style="width:25px" src="~/Content/refresh.png" alt="Alternate Text" />
                        </button>
                    }

                </div>
            </div>

            <div class="row" style="height:740px; padding:0px 30px 10px 30px;">

                @if (Model.paneType == "barcode")
                {
                    <div class="col-lg-6" style="display:none; padding:0px 20px 0px 0px;">
                        <div class="card" style="height: 100%;border: 2px solid #c0c2c4">
                            <div class="card-header text-center" style="        color: white;
        background-color: #c0c2c4;
        padding: 5px 0px 0px 0px">
                                <h4><b>History</b></h4>
                            </div>
                            <div class="card-body" style="padding: 15px 30px 0px 30px;">
                                <div class="row" style="margin-bottom:10px">
                                    <table class="table text-center" style="margin-top:5px">
                                        <thead class="text-center" style="background-color:#c0c2c4; color:white">
                                            <tr>
                                                <th style="padding:0px; width:300px">Station</th>
                                                <th style="padding:0px; width:400px">DATE</th>
                                                <th style="padding:0px ">STATUS</th>
                                            </tr>
                                        </thead>
                                        <tbody style="font-size: 25px;font-weight: bold">
                                            <tr>
                                                <td style="padding:10px">NDT</td>
                                                <td style="padding:10px">
                                                    @if (Model.barcodeInfo.ndt_machine_date.HasValue)
                                                    {
                                                        @Model.barcodeInfo.ndt_machine_date.Value.ToString("dddd dd, MMM yyyy")
                                                    }
                                                </td>
                                                <td style="padding:10px">@Model.barcodeInfo.ndt_machine_result</td>
                                            </tr>
                                            <tr>
                                                <td style="padding:10px">INITIAL INSPECTION</td>
                                                <td style="padding:10px">
                                                    @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                    {
                                                        @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("dddd dd, MMM yyyy")
                                                    }
                                                </td>
                                                <td style="padding:10px">@Model.barcodeInfo.preliminary_inspection_result</td>
                                            </tr>
                                            <tr>
                                                <td style="padding:10px">BUFFER BUILDER</td>
                                                <td style="padding:10px">
                                                    @if (Model.barcodeInfo.buffer_builder_date.HasValue)
                                                    {
                                                        @Model.barcodeInfo.buffer_builder_date.Value.ToString("dddd dd, MMM yyyy")
                                                    }
                                                </td>
                                                <td style="padding:10px">@Model.barcodeInfo.buffer_builder_result</td>
                                            </tr>
                                            <tr>
                                                <td style="padding:10px">FINAL INSPECTION</td>
                                                <td style="padding:10px">
                                                    @if (Model.barcodeInfo.final_inspection_date.HasValue)
                                                    {
                                                        @Model.barcodeInfo.final_inspection_date.Value.ToString("dddd dd, MMM yyyy")
                                                    }
                                                </td>
                                                <td style="padding:10px">@Model.barcodeInfo.final_inspection_result</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-3"></div>
                    <div class="col-lg-6" style="padding:0px">
                        <div class="card" style="border:2px solid #c0c2c4">
                            <div class="card-body" style="padding-top:20px">

                                <div class="row" style="padding:0px 15px 0px 15px">
                                    <div class="col-lg-10">
                                        <div class="row" style="padding-right:20px">
                                            <div class="col-lg-12 card shadow-lg" style="height:100%;">

                                                <div class="row" style="margin-top:10px">
                                                    <div class="col-lg-4 text-center" style="font-size:20px; padding-right:0px; color:#b4b6b8">
                                                        <b>Barcode Number</b>
                                                    </div>
                                                    <div class="col-lg-4 text-center" style="font-size:20px; color:#b4b6b8">
                                                        <b>Status</b>
                                                    </div>
                                                    <div class="col-lg-4 text-center" style="font-size:20px; padding-right:0px">
                                                    </div>
                                                </div>

                                                @using (Html.BeginForm("BufferBuilderBarcodeSave", "Retread", FormMethod.Post, new { htmlAttributes = new { @autocomplete = "off" } }))
                                                {
                                                    <div class="row" style="margin-bottom:30px">
                                                        <div class="col-lg-4" style="padding-right:0px">
                                                            @Html.EditorFor(m => m.barcodeInfo.barcode, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-weight:bold; font-size:30px; width:100%", @id = "ndt_barcode" } })
                                                        </div>
                                                        <div class="col-lg-4">
                                                            @Html.EditorFor(m => m.barcodeInfo.buffer_builder_result, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })

                                                        </div>
                                                        <div class="col-lg-2 text-left" style="padding:0px 15px 0px 0px">
                                                            @if (Model.barcodeInfo.final_inspection_result != null)
                                                            {
                                                                <input id="SubmitButtons" disabled type="submit" class="btn btn-success" style="width:100%; font-weight:bold; font-size:25px;" name="name" value="Submit" />
                                                            }
                                                            else
                                                            {
                                                                <input id="SubmitButton" type="submit" disabled class="btn btn-secondary" style="width:100%; font-weight:bold; font-size:25px;" name="name" value="Submit" />
                                                            }
                                                        </div>
                                                        <div class="col-lg-2 text-left" style="padding:0px 15px 0px 0px">
                                                            <input onclick="location.reload()" type="button" class="btn btn-danger" style="width:100%; font-weight:bold; font-size:25px;" name="name" value="Cancel" />
                                                        </div>
                                                    </div>
                                                    <div class="row" style="padding-bottom:0px; padding-left:15px; padding-right:15px">
                                                        <table class="table table-bordered text-center">
                                                            <thead class="text-center" style="background-color:#c0c2c4; color:white">
                                                                <tr>
                                                                    <th style="font-size:10px; width:80%; padding:0px">Customer Name</th>
                                                                    <th style="font-size:10px; padding:0px">Account#</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody style="font-size:20px; font-weight:bold">
                                                                <tr>
                                                                    <td style="padding:3px">
                                                                        @Model.customerName
                                                                    </td>
                                                                    <td style="padding:3px">
                                                                        @Model.workOrderInfo.customer_number
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <div class="row" style="padding-left:15px; padding-right:15px">
                                                        <table class="table table-bordered text-center">
                                                            <thead class="text-center" style="background-color:#c0c2c4; color:white">
                                                                <tr>
                                                                    <th style="font-size:10px; padding:0px">Tread Request</th>
                                                                    <th style="font-size:10px; padding:0px">Casing Size</th>
                                                                    <th style="font-size:10px; padding:0px">Casing Brand</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody style="font-size:20px; font-weight:bold">
                                                                <tr>
                                                                    <td style="padding:3px">
                                                                        @Html.EditorFor(m => m.barcodeInfo.retread_design, new { htmlAttributes = new { @readonly = true, @id = "treadInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })

                                                                    </td>
                                                                    <td style="padding:3px">
                                                                        @Html.EditorFor(m => m.barcodeInfo.casing_size, new { htmlAttributes = new { @readonly = true, @id = "sizeInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })

                                                                    </td>
                                                                    <td style="padding:3px">

                                                                        @Html.EditorFor(m => m.barcodeInfo.casing_brand, new { htmlAttributes = new { @readonly = true, @id = "brandInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })

                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>

                                                    <div class="row" style="padding-left:15px; padding-right:15px">
                                                        <table class="table table-bordered text-center">
                                                            <thead class="text-center" style="background-color:#c0c2c4; color:white">
                                                                <tr>
                                                                    <th style="font-size:10px; width:25%; padding:0px">Work Order#</th>
                                                                    <th style="font-size:10px; width:25%; padding:0px">Serial Dot</th>
                                                                    <th style="font-size:10px; width:25%; padding:0px">Location</th>
                                                                    <th style="font-size:10px; width:25%; padding:0px">Code</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody style="font-size:20px; font-weight:bold">
                                                                <tr>
                                                                    <td style="padding:3px">
                                                                        @Model.workOrderInfo.retread_workorder
                                                                    </td>
                                                                    <td style="padding:3px">
                                                                        @Model.barcodeInfo.serial_dot
                                                                    </td>
                                                                    <td style="padding:3px">
                                                                        @Html.EditorFor(m => m.barcodeInfo.ship_to_location, new { htmlAttributes = new { @readonly = true, @id = "shiptoInput", @style = "font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })
                                                                    </td>
                                                                    <td style="padding:3px">
                                                                        @Model.barcodeInfo.line_code
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <div class="row" style="margin-bottom:20px; padding-left:15px; padding-right:15px">
                                                        <div class="col-lg-12">
                                                            <div class="row">
                                                                <span style="font-size:20px; font-weight:bold">
                                                                    Comments
                                                                </span>
                                                            </div>
                                                            <div class="row">
                                                                @Html.TextAreaFor(m => m.barcodeInfo.comments, new { @style = "width:100%; height:170px", @oninput = "$('#SubmitButton').removeAttr('disabled').removeClass('btn-secondary').addClass('btn-success')" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                }

                                            </div>
                                        </div>
                                        <div class="row">

                                            <div class="col-lg-12" style="padding:2px 20px 0px 0px;">

                                                <div class="row" style="display:flex; margin:10px 0px 10px 0px; justify-content:space-evenly">
                                                    <img style="object-fit:contain; width:40px; margin-top:10px" src="~/Content/NDT.png" />
                                                    <img style="object-fit:contain; width:55px; margin-top:10px" src="~/Content/Prilim.png" />
                                                    <img style="object-fit:contain; width:40px; margin-top:10px" src="~/Content/Buffer.png" />
                                                    <img style="object-fit:contain; width:55px;" src="~/Content/Extensions.png" />
                                                    <img style="object-fit:contain; width:65px; margin-top:15px" src="~/Content/Builder.png" />
                                                    <img style="object-fit:contain; width:75px; margin-top:20px" src="~/Content/CuringChamber.png" />
                                                    <img style="object-fit:contain; width:60px; margin-top:10px" src="~/Content/Final.png" />
                                                </div>

                                                @{
                                                    string count = "0%";
                                                    string color = "green";

                                                    if (Model.barcodeInfo.preliminary_inspection_result == "GOOD" && Model.barcodeInfo.ndt_machine_result != "GOOD" && Model.barcodeInfo.buffer_builder_result != "GOOD" && Model.barcodeInfo.final_inspection_result != "GOOD")
                                                    {
                                                        count = "16.6%";
                                                    }
                                                    else if (Model.barcodeInfo.preliminary_inspection_result == "GOOD" && Model.barcodeInfo.ndt_machine_result == "GOOD" && Model.barcodeInfo.buffer_builder_result != "GOOD" && Model.barcodeInfo.final_inspection_result != "GOOD")
                                                    {
                                                        count = "28%";
                                                    }
                                                    else if (Model.barcodeInfo.preliminary_inspection_result == "GOOD" && Model.barcodeInfo.ndt_machine_result == "GOOD" && Model.barcodeInfo.buffer_builder_result == "GOOD" && Model.barcodeInfo.final_inspection_result != "GOOD")
                                                    {
                                                        count = "68%";
                                                    }
                                                    else if (Model.barcodeInfo.preliminary_inspection_result == "GOOD" && Model.barcodeInfo.ndt_machine_result == "GOOD" && Model.barcodeInfo.buffer_builder_result == "GOOD" && Model.barcodeInfo.final_inspection_result == "GOOD")
                                                    {
                                                        count = "100%";
                                                    }

                                                    if (Model.barcodeInfo.preliminary_inspection_result != null & Model.barcodeInfo.preliminary_inspection_result != "GOOD")
                                                    {
                                                        color = "green";
                                                    }

                                                    if (Model.barcodeInfo.final_inspection_result != null && Model.barcodeInfo.final_inspection_result == "GOOD")
                                                    {
                                                        color = "green";
                                                    }
                                                    else if (Model.barcodeInfo.buffer_builder_result != null && Model.barcodeInfo.buffer_builder_result == "GOOD")
                                                    {
                                                        color = "green";
                                                    }
                                                    else if (Model.barcodeInfo.ndt_machine_result != null && Model.barcodeInfo.ndt_machine_result == "GOOD")
                                                    {
                                                        color = "green";
                                                    }

                                                    if (color == "green")
                                                    {
                                                        <div class="progress" style="width:100%; height:35px;">
                                                            <div class="progress-bar progress-bar-striped bg-success" role="progressbar" aria-valuenow="70"
                                                                 aria-valuemin="0" aria-valuemax="100" style="width:@count">
                                                                <div class="row">
                                                                    @if (count == "16.6%")
                                                                    {
                                                                        <div class="col-lg-12">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                    }
                                                                    else if (count == "28%")
                                                                    {
                                                                        <div class="col-lg-6">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                        <div class="col-lg-6">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.ndt_machine_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.ndt_machine_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                    }
                                                                    else if (count == "68%")
                                                                    {
                                                                        <div class="col-lg-7">
                                                                            <div class="row">
                                                                                <div class="col-lg-5 text-right">
                                                                                    <b>
                                                                                        @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                                                        {
                                                                                            @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")
                                                                                        }
                                                                                    </b>
                                                                                </div>
                                                                                <div class="col-lg-5 text-left">
                                                                                    <b>
                                                                                        @if (Model.barcodeInfo.ndt_machine_date.HasValue)
                                                                                        {
                                                                                            @Model.barcodeInfo.ndt_machine_date.Value.ToString("MMM dd, yyyy")
                                                                                        }
                                                                                    </b>
                                                                                </div>
                                                                                <div class="col-lg-2"></div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-2"></div>
                                                                        <div class="col-lg-3">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.buffer_builder_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.buffer_builder_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                    }
                                                                    else if (count == "100%")
                                                                    {
                                                                        <div class="col-lg-12">
                                                                            <div class="row" style="display:flex">
                                                                                <b style="margin-left:40px">@Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")</b>
                                                                                <b style="margin-left:30px">@Model.barcodeInfo.ndt_machine_date.Value.ToString("MMM dd, yyyy")</b>
                                                                                <b style="margin-left:210px">@Model.barcodeInfo.buffer_builder_date.Value.ToString("MMM dd, yyyy")</b>
                                                                                <b style="margin-left:140px">@Model.barcodeInfo.final_inspection_date.Value.ToString("MMM dd, yyyy")</b>
                                                                            </div>
                                                                        </div>
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="progress" style="width:100%; height:35px;">
                                                            <div class="progress-bar progress-bar-striped bg-danger" role="progressbar" aria-valuenow="70"
                                                                 aria-valuemin="0" aria-valuemax="100" style="width:@count">
                                                                <div class="row">
                                                                    @if (count == "16.6%")
                                                                    {
                                                                        <div class="col-lg-12">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                    }
                                                                    else if (count == "28%")
                                                                    {
                                                                        <div class="col-lg-6">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                        <div class="col-lg-6">
                                                                            <b>
                                                                                @if (Model.barcodeInfo.ndt_machine_date.HasValue)
                                                                                {
                                                                                    @Model.barcodeInfo.ndt_machine_date.Value.ToString("MMM dd, yyyy")
                                                                                }
                                                                            </b>
                                                                        </div>
                                                                    }
                                                                    else if (count == "68%")
                                                                    {
                                                                        <div class="col-lg-7">
                                                                            <div class="row">
                                                                                <div class="col-lg-5 text-right">
                                                                                    <b>
                                                                                        @if (Model.barcodeInfo.preliminary_inspection_date.HasValue)
                                                                                        {
                                                                                            @Model.barcodeInfo.preliminary_inspection_date.Value.ToString("MMM dd, yyyy")
                                                                                        }
