    <script>
        function appendBarcode(val) {

            if (val == "10") {
                $('#ndt_barcode').val("");
                $('#ExistsIcon').css("display", "none")
                $('#DoesntExixtIcon').css("display", "none")
                $('#keyboardSubmit').prop({ disabled: true })
            }
            else if (val == "DEL") {
                let barcodeDel = $('#ndt_barcode').val();
                $('#ndt_barcode').val(barcodeDel.slice(0, -1));
                $('#ExistsIcon').css("display", "none")
                $('#DoesntExixtIcon').css("display", "none")
                $('#keyboardSubmit').prop({ disabled: true })
            }
            else
            {
                    if ($('#ndt_barcode').val().length < 8) {
                        let barcode = $('#ndt_barcode').val();
                        $('#ndt_barcode').val(barcode + val);
                    }
                    if ($('#ndt_barcode').val().length == 8) {

                        $.ajax({
                            url: '@Url.Action("validateBarcode", "Tools")',
                            data: { value: $('#ndt_barcode').val() },
                            type: 'POST',
                            success: function (data) {
                                if (data == "Exists") {
                                    $('#ExistsIcon').css("display", "block")
                                    $('#DoesntExixtIcon').css("display", "none")
                                    $('#keyboardSubmit').prop({ disabled: false })
                                }
                                else {
                                    $('#ExistsIcon').css("display", "none")
                                    $('#DoesntExixtIcon').css("display", "block")
                                    $('#keyboardSubmit').prop({ disabled: true })
                                }

                            }
                        });


                    }
                    else {
                        $('#ExistsIcon').css("display", "none")
                        $('#DoesntExixtIcon').css("display", "none")
                    }
            }
        }

    </script>


