@model SeHubPortal.ViewModel.PlantViewModel
@{
    ViewBag.Title = "Station";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap4.min.js"></script>

    <style>
        .breadCrumb{
            background-color:white;
        }
        #mainNavBar{
            display:none !important
        }

        #DashboardLink {
            display: block !important;
        }

        #PlantClicked {
            display: block !important;
        }


        #PlantBtn {
            font-weight: bold;
        }

        #PlantClicked {
            background-color: #f0f2f5;
            color: black;
            font-weight: 600;
        }

        #DashboardLink {
            color: black;
            font-weight: 700;
            background-color: #f0f2f5;
        }

        body {
            background-color: #f0f2f5;
            padding: 0px !important;
            margin: 0px !important;
        }


            .card-header h6 {
                color: black;
            }

        #Keyboard {
            padding-top: 6%;
        }

        #StatusModal {
            padding-right: 30%;
        }

        #TreadModal {
            padding-right: 25%
        }

        #SizesModal {
            padding-right: 10%
        }

        #ShiptoModal {
            padding-right: 4%
        }

        #BrandModel {
            padding-right: 25%
        }

        #consumablesModal {
            padding-right: 25%
        }

        .alerts-border {
            border: 3px crimson solid;
            animation: blink 1s;
            animation-iteration-count: 300;
        }

        @@keyframes blink {
            50% {
                border-color: white;
            }
        }

        @@media screen {
            #printSection {
                display: none;
            }

            #dontshow {
                display: none;
            }

            .dontshow {
                display: none;
            }
        }

        @@media print {

            @@page {
                margin: 0in;
            }

            .dontShowInPrint {
                display: none;
            }

            body * {
                visibility: hidden;
            }

            #printSection, #printSection * {
                visibility: visible;
            }

            #printSection {
                width:100%;
                height:100%;
                background-color:white;
                padding:0;
                margin:0;
                position: absolute;
                left: 0;
                top: 0;
            }

            html, body {
                height:100vh; 
                margin: 0 !important; 
                padding: 0 !important;
                overflow: hidden;
            }


        }

    </style>
</head>
<body style="overflow-y: hidden; overflow-x: hidden">

    <div class="row">
        <div class="col-md-12">
            <div class="row" style="padding:10px 30px 10px 30px">
                <div class="col-md-4" style="padding:10px; background-color:white;">
                    <a href="@Url.Action("Plant", "Retread")">Retread Plant</a> / @Model.station
                </div>
                <div class="col-md-4 text-center" style="padding:10px; background-color:white;">
                    @if (Model.paneType != "barcode")
                    {
                        <button id="keyboardbtn" data-target="#Keyboard" data-toggle="modal" class="btn btn-primary" style="padding: 2px 30px 2px 30px">
                            <b>Keyboard</b>
                        </button>
                    }
                </div>
                <div class="col-md-4" style="padding:10px; background-color:white;">

                    @if (Model.paneType == "barcode")
                    {
                        <button onclick="
                                            if ($('#SubmitButton').hasClass('alerts-border')) {
                                                $('#UnsavedChange').modal();
                                                return false
                                            };
                                            location.href = '@Url.Action("Station", new { pane = "", barcode = "", stationBarcode = Model.stationBarcode })';
                                        " style="float:right; background-color:transparent; outline:none; border:none">

                            <img style="width:25px" src="~/Content/refresh.png" alt="Alternate Text" />
                        </button>
                    }

                </div>
            </div>

            @if (Model.paneType == "barcode")
            {
                <div id="printThis" class="row dontshow" style="background-color:white; margin:5px">

                    <div style="display:flex; width:100%; justify-content:center; flex-direction:column; padding:0vw 20vw">
                        <img src="~/Content/dashboard.png" style="width:100%; object-fit:contain; margin-bottom:5vw; margin-top:17vw" alt="Alternate Text" />
                        <div class="text-center" style="padding:0px 10px; border:2px solid black; font-weight:bold; width:100%; font-size: 14vw">
                            @Model.barcodeInfo.casing_size
                        </div>
                        <div class="text-center" style="background-color: black;color: white;padding: 0px;border: 2px solid black;font-weight: bold;font-size: 14vw;margin-bottom: 8vw">
                            @if (Model.barcodeInfo.line_code == 5)
                            {
                                <span>SECTION</span>
                            }
                            else
                            {
                                <span>@Model.barcodeInfo.retread_design</span>
                            }
                        </div>

                        <div class="text-center" style="padding: 0px;font-weight: bold; display:flex; justify-content:space-evenly; align-items:center; background-color:black; color:white">
                            <span style="font-weight:bold; font-size:7px; width:50%; border:1px solid dimgray; padding:2px">Barcode</span>
                            <span style="font-weight:bold; font-size:7px; width:50%; border:1px solid dimgray; padding:2px">WorkOrder</span>
                        </div>
                        <div class="text-center" style="padding: 0px;font-weight: bold; margin-bottom:3vw; display:flex; justify-content:space-evenly; align-items:center;">
                            <span style="font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">@Model.barcodeInfo.barcode</span>
                            <span style="font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">@Model.workOrderInfo.retread_workorder</span>
                        </div>

                        <div class="text-center" style="padding: 0px;font-weight: bold; display:flex; justify-content:space-evenly; align-items:center; background-color:black; color:white">
                            <span style="font-weight:bold; font-size:7px; width:50%; border:1px solid dimgray; padding:2px">P/N (s)</span>
                            <span style="font-weight:bold; font-size:7px; width:50%; border:1px solid dimgray; padding:2px">Cap Count</span>
                        </div>
                        <div class="text-center" style="padding: 0px;font-weight: bold; margin-bottom:3vw; display:flex; justify-content:space-evenly; align-items:center;">
                            @if (Model.barcodeInfo.line_code == 5 || Model.barcodeInfo.line_code == 1 || Model.barcodeInfo.line_code == 6)
                            {
                                <span style="font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">
                                    TBD
                                </span>
                            }
                            else
                            {
                                <span style="visibility:hidden; font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">
                                    TBD
                                </span>
                            }
                            <span style="font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">@Model.barcodeInfo.cap_count</span>
                        </div>

                        <div class="text-center" style="padding: 0px;font-weight: bold; display:flex; justify-content:space-evenly; align-items:center; background-color:black; color:white">
                            <span style="font-weight:bold; font-size:7px; width:50%; border:1px solid dimgray; padding:2px">Creation</span>
                            <span style="font-weight:bold; font-size:7px; width:50%; border:1px solid dimgray; padding:2px">Production</span>
                        </div>
                        <div class="text-center" style="padding: 0px;font-weight: bold; margin-bottom:3vw; display:flex; justify-content:space-evenly; align-items:center;">
                            <span style="font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">
                                @if (Model.barcodeInfo.TT050_date.HasValue)
                                {
                                    @Model.barcodeInfo.TT050_date.Value.ToString("yyyy-MM-dd")
                                }
                            </span>
                            <span style="font-weight:bold; font-size:13px; width:50%; border:1px solid black; padding:2px">
                                @if (Model.barcodeInfo.TT600_date.HasValue)
                                {
                                    @Model.barcodeInfo.TT600_date.Value.ToString("yyyy-MM-dd")
                                }
                            </span>
                        </div>

                        <img src="https://ctasehub.blob.core.windows.net/sehub-content/MadeInNewfoundland.png" style="margin-top:3vw; width:100%; object-fit:contain; margin-bottom:3vw" alt="Alternate Text" />


                        <div class="text-center" style="padding:0px; border:2px solid black; font-weight:bolder; font-size:11vw; margin-top:3vw">
                            <span>
                                @if (Model.barcodeInfo.line_code == 6)
                                {
                                    <span>STOCK</span>
                                }
                                else
                                {
                                    if (Model.customerName.Length > 11)
                                    {
                                        @Model.customerName.Substring(0, 11)
                                    }
                                    else
                                    {
                                        @Model.customerName
                                    }
                                }
                            </span>
                        </div>

                        <div class="text-center" style="background-color: black; padding: 0px; border: 2px solid black;">
                            <h1 style="font-size:22vw; font-weight:bolder; margin:0; color:white">@Model.barcodeInfo.ship_to_location</h1>
                        </div>
                    </div>
                </div>


            }
            else if (false)
            {
                <div id="printThis" class="row dontshow" style="margin:70px; padding:200px; background-color:white">
                    <div class="col-md-12" style="">
                        <div class="row" style="margin-bottom:40px">
                            <div class="col-md-12 text-center">
                                <img src="~/Content/dashboard.png" style="width:100%" alt="Alternate Text" />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 text-center" style="padding:10px; border:4px solid black; font-weight:bold; font-size:180px">
                                @Model.barcodeInfo.casing_size
                            </div>
                        </div>
                        <div class="row" style="margin-bottom:30px">
                            <div class="col-md-12 text-center" style="background-color: black;color: white;padding: 0px;border: 2px solid black;font-weight: bold;font-size: 250px">
                                @if (Model.barcodeInfo.line_code == 5)
                                {
                                    <span>SECTION</span>
                                }
                                else
                                {
                                    <span>@Model.barcodeInfo.retread_design</span>
                                }
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 text-center" style="color:black; font-size:50px">
                                <div class="row" style="margin-bottom:10px">
                                    <div class="col-md-12 text-center" style="padding-right:7px; font-weight:bold">
                                        Barcode #: <span style="font-size:70px; font-weight:bolder">@Model.barcodeInfo.barcode</span>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:10px">
                                    <div class="col-md-12 text-center" style="padding-right: 7px; font-weight:bold">
                                        Work Order #: <span style="font-size:70px; font-weight:bolder">@Model.workOrderInfo.retread_workorder</span>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:10px">
                                    <div class="col-md-12 text-center" style="padding-right: 7px; font-weight:bold">
                                        Casing Dot: <span style="font-size:70px; font-weight:bolder">@Model.barcodeInfo.serial_dot</span>
                                    </div>
                                </div>
                            </div>
                        </div>



                        <div class="row" style="margin-bottom:30px">
                            <div class="col-md-12 text-center" style="color:black; font-size:50px">
                                <div class="row" style="margin-bottom:10px">
                                    <div class="col-md-12 text-center" style="padding-right:7px; font-weight:bold">
                                        Inspection Date: <span style="font-size:70px; font-weight:bolder">
                                            @if (Model.barcodeInfo.TT050_date.HasValue)
                                            {@Model.barcodeInfo.TT050_date.Value.ToString("yyyy-MM-dd")}
                                        </span>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:10px">
                                    <div class="col-md-12 text-center" style="padding-right: 7px; font-weight:bold">
                                        Production Date: <span style="font-size:70px; font-weight:bolder">@System.DateTime.Today.ToString("yyyy-MM-dd")</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 text-center" style="padding:10px; border:4px solid black; font-weight:bolder; font-size:105px">
                                @if (Model.barcodeInfo.line_code == 6)
                                {
                                    <span>Casing Credit</span>
                                }
                                else
                                {
                                    if (Model.customerName.Length > 11)
                                    {
                                        @Model.customerName.Substring(0, 11)
                                    }
                                    else
                                    {
                                        @Model.customerName
                                    }
                                }
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 text-center" style="background-color:black; padding:0px; border:2px solid black;">
                                <h1 style="font-size:600px; font-weight:bolder; margin:0; color:white">@Model.barcodeInfo.ship_to_location</h1>
                            </div>
                        </div>
                    </div>
                </div>
            }

            <div class="row" style="padding:0px 30px 20px 30px">

                @if (Model.paneType == "barcode")
                {
                    <div style="display:flex; justify-content:center; width:100%">
                        @*margin-top:5em; *@
                        <div>
                            <div class="card" style="border:2px solid #c0c2c4">
                                <div class="card-body" style="padding-top:15px; padding-bottom:5px">
                                    <div class="row" style="padding:0px 15px 0px 15px">
                                        <div class="col-lg-10">
                                            <div class="row" style="padding-right:20px">
                                                <div class="col-lg-12 card shadow-lg" style="height:100%; padding-top:15px;">
                                                    <div style="width:100%; display:flex">
                                                        <div style="flex:3; display:flex; justify-content:center; align-items:center">
                                                            <b>Barcode Number</b>
                                                        </div>
                                                        <div style="flex:3; display:flex; justify-content:center">
                                                            <b>Status</b>
                                                        </div>
                                                        <div style="flex:3; display:flex; justify-content:center">

                                                        </div>
                                                    </div>


                                                    @using (Html.BeginForm("BarcodeSave", "Retread", FormMethod.Post, new { htmlAttributes = new { @autocomplete = "off" } }))
                                                    {
                                                        @Html.HiddenFor(m => m.station)
                                                        @Html.HiddenFor(m => m.stationBarcode)
                                                        <div class="row" style="margin-bottom:30px">

                                                            <div style="width:100%; display:flex; padding:0px 15px">
                                                                <div style="flex:3; display:flex; justify-content:center; align-items:center; padding-right:5px">
                                                                    @Html.EditorFor(m => m.barcodeInfo.barcode, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-weight:bold; font-size:30px; width:100%", @id = "ndt_barcode" } })
                                                                </div>
                                                                <div style="        flex: 3;
        display: flex;
        justify-content: center;
        padding: 0px 5px">
                                                                    @if (Model.station == "Preliminary")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT050_result, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                    else if (Model.station == "NDT")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT100_result, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                    else if (Model.station == "Buffer")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT200_result, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                    else if (Model.station == "Repair")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT300_result, new { htmlAttributes = new { @disabled = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                    else if (Model.station == "Builder")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT400_result, new { htmlAttributes = new { @disabled = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                    else if (Model.station == "Chamber")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT500_result, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                    else if (Model.station == "Final")
                                                                    {
                                                                        @Html.EditorFor(m => m.barcodeInfo.TT600_result, new { htmlAttributes = new { @readonly = true, @autocomplete = "off", @style = "text-align:center; font-size:30px; font-weight:bold; width:100%", @id = "statusInput" } })
                                                                    }
                                                                </div>
                                                                <div style="flex:3; display:flex; padding-left:5px">
                                                                    <div style="flex:1; display:flex; justify-content:center; padding-right:5px">
                                                                        @if (Model.workStations.Where(x => x.station == Model.station).Select(x => x.status).FirstOrDefault() == 1)
                                                                        {
                                                                            <input id="SubmitButton" type="submit" disabled class="btn btn-secondary" style="width:100%; font-weight:bold; font-size:25px;" name="name" value="Save" />
                                                                        }
                                                                        else
                                                                        {
                                                                            <input id="SubmitButton" type="submit" disabled class="btn btn-secondary" style="width:100%; font-weight:bold; font-size:25px;" name="name" value="Save" />
                                                                        }
                                                                    </div>
                                                                    <div style=" flex: 1; display: flex; justify-content: center; padding-left: 5px ">
                                                                        <input onclick="location.reload()" type="button" class="btn btn-danger" style="width:100%; font-weight:bold; font-size:25px;" name="name" value="Cancel" />
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                        <div class="row" style="padding-bottom: 10px;padding-left: 15px;padding-right: 15px">
                                                            <table class="table table-bordered text-center">
                                                                <thead class="text-center" style="background-color: #c0c2c4; color: white">
                                                                    <tr>
                                                                        <th style="font-size: 10px; padding: 0px; width: 50%">Customer Name</th>
                                                                        <th style="font-size: 10px; padding: 0px; width: 12.5%">Account#</th>
                                                                        <th style="font-size: 10px; padding: 0px; width: 12.5%">Brand Only</th>
                                                                        <th style="font-size: 10px; padding: 0px; width: 12.5%">Age Limit</th>
                                                                        <th style="font-size: 10px; padding: 0px; width: 12.5%">Buffer Spec</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody style="font-size:20px; font-weight:bold">
                                                                    <tr>
                                                                        <td style="padding:3px">
                                                                            @Model.customerName
                                                                        </td>
                                                                        <td style="padding:3px">
                                                                            @Model.workOrderInfo.customer_number
                                                                        </td>
                                                                        <td style="padding:3px">
                                                                            @if (Model.BrandRequirements != null)
                                                                            {
                                                                                <span>@Model.BrandRequirements</span>
                                                                            }
                                                                            else
                                                                            {
                                                                                <span>N/A</span>
                                                                            }
                                                                        </td>
                                                                        <td style="padding:3px">
                                                                            @if (Model.AgeLimit != null)
                                                                            {
                                                                                <span>@Model.AgeLimit</span>
                                                                            }
                                                                            else
                                                                            {
                                                                                <span>N/A</span>
                                                                            }
                                                                        </td>
                                                                        <td style="padding:3px">
                                                                            @if (Model.buffer_spec != null)
                                                                            {
                                                                                <span>@Model.buffer_spec</span>
                                                                            }
                                                                            else
                                                                            {
                                                                                <span>N/A</span>
                                                                            }
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                        <div class="row" style="padding-bottom:10px; padding-left:15px; padding-right:15px">
                                                            <table class="table table-bordered text-center">
                                                                <thead class="text-center" style="background-color:#c0c2c4; color:white">
                                                                    <tr>
                                                                        <th style="font-size:10px; padding:0px; width:25%">Tread Request</th>
                                                                        <th style="font-size:10px; padding:0px; width:25%">Casing Size</th>
                                                                        <th style="        font-size: 10px;
        padding: 0px;
        width: 25%">Casing Brand</th>
                                                                        <th style="font-size:10px; padding:0px; width:25%">Cap Count</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody style="font-size: 20px; font-weight: bold">
                                                                    <tr>
                                                                        <td style="padding:3px">
                                                                            @Html.EditorFor(m => m.barcodeInfo.retread_design, new { htmlAttributes = new { @readonly = true, @id = "treadInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })
                                                                        </td>
                                                                        @if (Model.station == "NDT" && Model.barcodeInfo.casing_size == null)
                                                                        {
                                                                            <td style="padding:3px; border:3px solid red" id="casing_size_td">
                                                                                @Html.EditorFor(m => m.barcodeInfo.casing_size, new { htmlAttributes = new { @readonly = true, @id = "sizeInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })
                                                                            </td>
                                                                        }
                                                                        else
                                                                        {
                                                                            <td style="padding:3px;">
                                                                                @Html.EditorFor(m => m.barcodeInfo.casing_size, new { htmlAttributes = new { @readonly = true, @id = "sizeInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })
                                                                            </td>
                                                                        }
                                                                        <td style="padding:3px">
                                                                            @Html.EditorFor(m => m.barcodeInfo.casing_brand, new { htmlAttributes = new { @readonly = true, @id = "brandInput", @style = "width:100%; font-weight:bold; background-color:transparent; text-align:center; border:none; outline:none" } })
                                                                        </td>
